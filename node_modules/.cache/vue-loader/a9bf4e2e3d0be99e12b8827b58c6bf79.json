lay: flex;\n  }\n\n  .topbox {\n    .box {\n      .pop {\n        float: right;\n      }\n\n      .icon-woderenqi {\n        float: left;\n        font-size: 50px;\n        margin-right: 20px;\n      }\n\n      .icon-shang {\n        float: right;\n        font-size: 36px;\n        margin-left: 20px;\n        color: #ffe554;\n      }\n\n      .total {\n        float: left;\n      }\n\n      padding-left: 40px;\n      overflow: hidden;\n      width: 690px;\n      height: 120px;\n      border-radius: 10px;\n      background: #365cf6;\n      text-align: center;\n      line-height: 120px;\n      font-size: 26px;\n      color: #fff;\n      box-sizing: border-box;\n      padding-right: 60px;\n    }\n\n    height: 195px;\n    width: 100%;\n    box-sizing: border-box;\n    border-bottom: 20px solid #f6f6f6;\n    padding: 24px 30px;\n  }\n\n  color: #000;\n  width: 100%;\n  box-sizing: border-box;\n  min-height: 100vh;\n  background: #fff;\n  overflow: hidden;\n  position: relative;\n}\n",{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8FA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/module/message/popular","sourcesContent":["<template>\n  <Wrapper :paddingTop=\"paddingTop\" class=\"warpper\" @scrolling=\"nextPage\">\n    <AppHeader ref=\"header\" title=\"人气\"></AppHeader>\n    <!-- 顶部bar -->\n    <div class=\"topbox\">\n      <div class=\"box\"><i class=\"iconfont icon-woderenqi\"></i><span class=\"total\">总人气 {{all}}</span><i class=\"iconfont icon-shang\"></i><span class=\"pop\">今日人气 {{today}}</span></div>\n    </div>\n    <!-- 列表数据 -->\n    <div class=\"list\" v-for=\"(item,index) in lists\" :key=\"index\" @click=\"toPage(item.user_id)\">\n      <img class=\"avatar\" :src=\"item.user_id?(item.head_portrait||item.head_picture):'http://weixin.yuhongrocky.top/static/logo.png'\" />\n      <span class=\"name\">{{item.user_id?(item.wechat_name||item.user_name):'游客'}}</span>\n      <span class=\"time\">{{item.created_at| timeToNow}}</span>\n    </div>\n    <p class=\"none\" v-show=\"lists.length==0&&!firstIn\">\n      <img src=\"./renqi.jpg\" class=\"pic\" />\n    </p>\n    <p class=\"tips\">{{loading?'正在加载...':lists.length>7?'暂无更多':''}}</p>\n  </Wrapper>\n</template>\n<script>\nimport dialogService from '../../../service/dialogService'\nimport messageApi from '../../../api/message/index'\nimport AppLoading from '../../../components/AppLoading/index'\nimport AppHeader from '../../../components/AppHeader'\nimport Wrapper from '../../../components/Wrapper/index'\nimport { mapState } from 'vuex'\nexport default {\n  name: 'message-popular',\n  data() {\n    return {\n      loading: false, // 是否在加载\n      windowHeight: null, // 可见区域高度\n      lists: [], //列表数据\n      last_page: null, // 总页数\n      page: 1, // 当前页数\n      firstIn: true,\n      today: '--', // 今日人气\n      all: '--' //总人气\n    }\n  },\n  components: {\n    AppLoading,\n    AppHeader,\n    Wrapper\n  },\n  async mounted() {\n    this.getlist()\n  },\n  computed: {\n    ...mapState({\n      platform: state => state.system.platform\n    }),\n    paddingTop: function() {\n      return this.platform === 'wechat' ? '0px' : '100px'\n    }\n  },\n  methods: {\n    // 去名片\n    async toPage(id) {\n      if (!id) {\n        dialogService.toast('平台外用户访问了您')\n        return\n      }\n      this.$router.push(`/card/detail/${id}`)\n    },\n    // 获取列表\n    async getlist(load) {\n      this.loading = true\n      if (load) this.page += 1\n      let msg = await messageApi.getPoplist(this.page)\n      if (load) {\n        this.lists = this.lists.concat(msg.data.list.data)\n      } else {\n        this.lists = msg.data.list.data\n        this.all = msg.data.all\n        this.today = msg.data.today\n        this.firstIn = false\n      }\n      this.last_page = msg.data.list.last_page\n      this.windowHeight = document.documentElement.clientHeight\n      this.loading = false\n    },\n    nextPage(scrolled, scrollHeight) {\n      let scrollBottom = this.windowHeight + scrolled > scrollHeight - 50\n      if (!scrollBottom || this.loading || this.page >= this.last_page) {\n        return false\n      } else {\n        this.getlist(true)\n      }\n    }\n  }\n}\n</script>\n<style lang=\"scss\" scoped>\n.warpper {\n  .tips {\n    font-size: 28px;\n    text-align: center;\n    margin-top: 40px;\n    padding-bottom: 20px;\n  }\n\n  .none {\n    text-align: center;\n    padding-top: 100px;\n\n    .pic {\n      width: 348px;\n      height: 349px;\n    }\n  }\n\n  .list {\n    .avatar {\n      width: 90px;\n      height: 90px;\n      border-radius: 50%;\n      margin-right: 25px;\n    }\n\n    .name {\n      flex: 1;\n      color: #000;\n      font-size: 30px;\n      /* font-weight: bold; */\n    }\n\n    .time {\n      font-size: 22px;\n      color: #666;\n    }\n\n    align-items: center;\n    width: 100%;\n    border-bottom: 1px solid #f5f5f5;\n    height: 150px;\n    box-sizing: border-box;\n    padding: 0 30px;\n    line-height: 50px;\n    display: flex;\n  }\n\n  .topbox {\n    .box {\n      .pop {\n        float: right;\n      }\n\n      .icon-woderenqi {\n        float: left;\n        font-size: 50px;\n        margin-right: 20px;\n      }\n\n      .icon-shang {\n        float: right;\n        font-size: 36px;\n        margin-left: 20px;\n        color: #ffe554;\n      }\n\n      .total {\n        float: left;\n      }\n\n      padding-left: 40px;\n      overflow: hidden;\n      width: 690px;\n      height: 120px;\n      border-radius: 10px;\n      background: #365cf6;\n      text-align: center;\n      line-height: 120px;\n      font-size: 26px;\n      color: #fff;\n      box-sizing: border-box;\n      padding-right: 60px;\n    }\n\n    height: 195px;\n    width: 100%;\n    box-sizing: border-box;\n    border-bottom: 20px solid #f6f6f6;\n    padding: 24px 30px;\n  }\n\n  color: #000;\n  width: 100%;\n  box-sizing: border-box;\n  min-height: 100vh;\n  background: #fff;\n  overflow: hidden;\n  position: relative;\n}\n</style>"]}]}