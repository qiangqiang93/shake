{ Wrapper, AppLoading },\n  async mounted() {\n    this.getlist()\n  },\n  methods: {\n    // 获取列表\n    async getlist(load) {\n      let that = this\n      this.loading = true\n      if (load) this.page += 1\n      let res = await messageApi.getUserdetail(this.$route.query.id)\n      if (load) {\n        res.data.list.data.forEach(element => {\n          that.lists.unshift(element)\n        })\n        this.$nextTick(() => {\n          that.windowHeight = document.documentElement.scrollHeight\n          window.scrollTo(0, that.windowHeight - that.last_height)\n          that.last_height = that.windowHeight\n        })\n      } else {\n        this.lists = res.data.list.data.reverse()\n        this.$nextTick(() => {\n          window.scrollTo(0, document.documentElement.scrollHeight)\n          this.last_height = document.documentElement.scrollHeight\n          this.firstIn = false\n        })\n      }\n      this.last_page = res.data.list.last_page\n      this.windowHeight = document.documentElement.clientHeight\n      this.loading = false\n    },\n    nextPage(scrolled, scrollHeight) {\n      let scrollTop = scrolled < 1\n      if (!scrollTop || this.loading || this.page >= this.last_page) {\n        return false\n      } else {\n        this.getlist(true)\n      }\n    }\n  }\n}\n</script>\n<style lang=\"scss\" scoped>\n.msg-detail-tips {\n  font-size: 28px;\n  text-align: center;\n  margin-top: 40px;\n  padding-bottom: 20px;\n}\n.warpper {\n  .tips {\n    font-size: 28px;\n    text-align: center;\n    margin-top: 40px;\n    padding-bottom: 20px;\n  }\n  .message {\n    .box {\n      display: flex;\n      .avatar {\n        width: 80px;\n        height: 80px;\n        border-radius: 50%;\n        margin-right: 30px;\n      }\n      .ctbox {\n        .toggle {\n          .button {\n            width: 145px;\n            height: 50px;\n            box-sizing: border-box;\n            font-size: 26px;\n            line-height: 50px;\n            text-align: center;\n