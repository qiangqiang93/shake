dit\" class=\"card-card-logo-upload\" ratio=\"1\" @success=\"success\"></AppUploadImg>\n      </div>\n      <div class=\"card-card-info\">\n        <p class=\"card-card-name\" v-text=\"item.name || item.user_name\"></p>\n        <p class=\"card-card-works\" v-text=\"item.profession\"></p>\n      </div>\n    </div>\n    <div class=\"card-card-concat\">\n      <p v-if=\"item.company\" class=\"card-card-company\" v-text=\"item.company\"></p>\n      <p v-if=\"item.phone || item.mobile_phone\" class=\"card-card-phone\">电话: <span v-text=\"item.phone || item.mobile_phone\"></span></p>\n      <p v-if=\"item.wechat || item.wechat_code\" class=\"card-card-wechat\">微信: <span v-text=\"item.wechat || item.wechat_code\"></span></p>\n      <p v-if=\"item.address + item.address_detail\" class=\"card-card-address\">地址: <span v-text=\"item.address + item.address_detail\"></span></p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport AppUploadImg from '../../components/AppUploadImg'\nexport default {\n  props: ['item', 'edit'],\n  data() {\n    return {}\n  },\n  methods: {\n    // 上传成功\n    success(path) {\n      this.$emit('upload', path)\n    }\n  },\n  components: {\n    AppUploadImg\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n@import './index.scss';\n</style>\n"]}]}