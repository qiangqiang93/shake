;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8PA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/module/login/index","sourcesContent":["<template>\n  <div class=\"login\">\n    <h3>要流量,上汇脉宝</h3>\n    <div class=\"content-warp\" :class=\"selectIndex?'pwd':''\">\n      <div class=\"title\">\n        <div :class=\"!selectIndex?'active':''\" @click=\"select(0)\">短信登录</div>\n        <div :class=\"selectIndex?'active':''\" @click=\"select(1)\">密码登录</div>\n      </div>\n      <div class=\"content\">\n        <!-- 短信登录 -->\n        <transition name=\"slide-fade\">\n          <div v-if=\"selectIndex === 0\">\n            <div class=\"input-group content-item\"><input placeholder=\"输入手机号\" v-model=\"phone\" /> </div>\n            <div class=\"input-group content-item m-b5\"><input placeholder=\"输入验证码\" v-model=\"code\" />\n              <span class=\"tips\" :class=\"active?'':'dis'\" @click=\"getCode\">{{tips}}</span>\n            </div>\n            <button class=\"btn\" ref=\"loginBtn\" @click=\"login\" :class=\"isRequest?'under-way':''\">{{isRequest?'登录中...':'登录'}}</button>\n            <div class=\"content-item1\">\n              <span class=\"tips\" @click=\"goRegiste\" style=\"width:100%\">一键注册</span>\n            </div>\n          </div>\n        </transition>\n        <!-- 密码登录 -->\n        <transition name=\"slide-fade1\">\n          <div v-if=\"selectIndex === 1\">\n            <div class=\"input-group content-item\"><input v-model=\"phone\" placeholder=\"输入手机号\" /> </div>\n            <div class=\"input-group content-item m-b5\"><input v-model=\"password\" placeholder=\"输入密码\" type=\"password\" /></div>\n            <button class=\"btn\" ref=\"loginBtn\" @click=\"login\" :class=\"isRequest?'under-way':''\">{{isRequest?'登录中...':'登录'}}</button>\n            <div class=\"content-item1\">\n              <router-link tag=\"span\" to=\"/forgetPwd\">忘记密码</router-link>\n              <span class=\"tips\" :class=\"active?'':'dis'\" @click=\"goRegiste\">一键注册</span>\n            </div>\n          </div>\n        </transition>\n      </div>\n    </div>\n    <!-- app显示的微信登录 -->\n    <div class=\"wechat-login\" v-if=\"system.platform!=='wechat'\">\n      <div class=\"wechat-tip\"></div>\n      <img src=\"./wechat.png\" alt=\"\" @click=\"wechatLogin\">\n    </div>\n    <p class=\"footer\">注册登录即同意《汇脉宝协议》</p>\n  </div>\n</template>\n\n<script>\nimport Login from '../../../api/login'\nimport User from '../../../api/user'\nimport System from '../../../api/system'\nimport dialogService from '../../../service/dialogService.js'\nimport validate from '../../../utils/validate'\nimport { mapState, mapActions } from 'vuex'\n\nexport default {\n  name: 'loginIndex',\n  data() {\n    return {\n      selectIndex: 0, // 默认选中短信\n      phone: '', //电话\n      password: '', //密码\n      code: '', // 验证码\n      tips: '获取验证码', // 验证码提示\n      active: true, // 是否可点击发送验证码\n      timer: false,\n      isRequest: false, // 是否在请求\n      getWechatTokenParams: {\n        // 换取微信token请求的参数\n        appid: 'wx159e6dce2e784f2a',\n        secret: 'ac83e6a5151e45de1ff40adfd27660df',\n        code: ''\n      }\n    }\n  },\n  beforeMount() {\n    // 如果有token，则跳转到首页\n    let token = this.system.token\n    if (token) {\n      this.$router.replace('/index')\n    }\n  },\n  computed: {\n    ...mapState(['system', 'user'])\n  },\n  methods: {\n    ...mapActions([\n      'system/setToken',\n      'system/updateRedirect',\n      'user/setWechatInfo'\n    ]),\n    select(index) {\n      this.selectIndex = index\n      clearInterval(this.timer)\n      this.code = ''\n      this.phone = ''\n      this.password = ''\n      this.tips = '获取验证码'\n      this.active = true\n    },\n    // 获取验证码 type发送验证码 1登录 2注册 3改密 4换绑 5设置支付密码\n    async getCode() {\n      if (!this.active) return\n      let check = validate.phone(this.phone)\n      if (!check.status) {\n        dialogService.toast(check.msg)\n        return false\n      }\n      try {\n        let res = await System.sendValidateCode(this.phone, 1)\n        if (res.status !== 0) throw new Error(res.message)\n        dialogService.toast('发送成功')\n        this.countdown()\n      } catch (err) {\n        dialogService.toast(err.message)\n      }\n    },\n    // 倒计时\n    countdown() {\n      let second = 60\n      this.active = false\n      this.tips = second + 's'\n      this.timer = setInterval(() => {\n        second--\n        this.tips = second + 's'\n        if (second === 0) {\n          clearInterval(this.timer)\n          this.active = true\n          this.tips = '获取验证码'\n        }\n      }, 1000)\n    },\n    // 登录\n    async login() {\n      if (this.isRequest) return false\n      try {\n        let phoneValidate = validate.phone(this.phone)\n        if (!phoneValidate.status) {\n          throw new Error(phoneValidate.msg)\n        }\n        let res = null\n        if (this.selectIndex == 1) {\n          // 密码登录\n          if (!this.password.length) {\n            throw new Error('密码不能为空')\n          }\n          if (this.password.length < 6 || this.password.length > 12)\n            throw new Error('密码长度为6-12位')\n          this.isRequest = true\n          res = await Login.pwdLogin({\n            phone: this.phone,\n            password: this.password\n          })\n        } else {\n          // 短信登录\n          if (!this.code.length) {\n            throw new Error('验证码不能为空')\n          }\n          this.isRequest = true\n          res = await Login.codeLogin({\n            phone: this.phone,\n            code: this.code\n          })\n        }\n        if (res.status === -1) {\n          throw new Error(res.message)\n        }\n        this['system/setToken'](res.data.token)\n        await User.getProfile()\n        this.isRequest = false\n        // 是否完善信息\n        if (!this.user.userInfo.is_per) {\n          this.$router.replace('/information')\n          return false\n        }\n\n        this.$router.push('/index')\n      } catch (error) {\n        this.isRequest = false\n\n        dialogService.toast(error.message)\n      }\n    },\n    goRegiste() {\n      if (this.system.platform !== 'wechat') {\n        this.$router.push('/app/register')\n      } else {\n        let guider = this.system.guider || ''\n        location.href =\n          window.api +\n          '/front/oauth?user_id=' +\n          guider +\n          '&redirect=' +\n          encodeURIComponent(this.system.redirect)\n      }\n    },\n    // 微信登录\n    async wechatLogin() {\n      try {\n        // 先授权\n        let res = await app.weChatAuth()\n        this.getWechatTokenParams.code = res.code\n        // 获取网页授权access_token\n        let ress = await Login.getAccessToken(this.getWechatTokenParams)\n        // 拉取微信信息\n        let resInfo = await Login.getWechatInfo(ress)\n        this['user/setWechatInfo'] = resInfo\n        let resLogin = await Login.wechatLogin({\n          union_id: resInfo.unionid\n        })\n        // 还没绑定手机\n        if (resLogin.message.indexOf('需要绑定手机号码') > -1) {\n          this.$router.push('/bindPhone')\n        }\n        if (resLogin.status === 0) {\n          this['system/setToken'](resLogin.data.token)\n          await User.getProfile()\n          this.$router.push('/index')\n        }\n      } catch (error) {\n        dialogService.toast(error.message)\n      }\n    }\n  },\n  beforeRouteLeave(to, from, next) {\n    // this.system.token\n    let nameArr = ['/forgetPwd', '/register', '/app/register']\n    if (nameArr.indexOf(to.path) === -1) {\n      if (!this.system.token) {\n        if (typeof WeixinJSBridge != 'undefined') {\n          WeixinJSBridge.call('closeWindow')\n        } else {\n          if (navigator.userAgent.indexOf('MSIE') > 0) {\n            if (navigator.userAgent.indexOf('MSIE 6.0') > 0) {\n              window.opener = null\n              window.close()\n            } else {\n              window.open('', '_top')\n              window.top.close()\n            }\n          } else if (navigator.userAgent.indexOf('Firefox') > 0) {\n            window.location.href = 'about:blank '\n          } else {\n            window.opener = null\n            window.open('', '_self', '')\n            window.close()\n          }\n        }\n        next(false)\n      }\n    }\n    next()\n  }\n}\n</script>\n<style lang=\"scss\" scoped>\n@import '../index.scss';\n\n.login {\n  overflow: hidden;\n  min-height: 100vh;\n  background: #fff;\n\n  h3 {\n    font-size: 40px;\n    font-weight: bold;\n    font-stretch: normal;\n    color: #000000;\n    padding-left: 40px;\n    box-sizing: border-box;\n    margin-top: 77px;\n  }\n\n  .content-warp {\n    position: relative;\n    margin: 78px auto 0;\n    width: 690px;\n    height: 664px;\n    background: url('./left.png') no-repeat;\n    background-size: cover;\n\n    &.pwd {\n      background: url('./right.png') no-repeat;\n      background-size: cover;\n    }\n  }\n\n  .title {\n    display: flex;\n    font-size: 30px;\n    color: #666666;\n    height: 120px;\n\n    > div {\n      flex: 1;\n      margin-top: 56px;\n      text-align: center;\n\n      &.active {\n        color: #274ff3;\n      }\n    }\n  }\n\n  .content {\n    .content-item {\n      &:nth-last-child(1) {\n        margin-bottom: 0;\n        height: auto;\n        padding: 0;\n        background-color: #fff;\n      }\n    }\n  }\n\n  .footer {\n    font-size: 20px;\n    line-height: 30px;\n    color: #666666;\n    text-align: center;\n    position: absolute;\n    bottom: 40px;\n    width: 100%;\n  }\n\n  .slide-fade-enter-active,\n  .slide-fade1-enter-active {\n    transition: all 0.4s ease;\n  }\n\n  .slide-fade-enter {\n    transform: translateX(-390px);\n  }\n\n  .slide-fade1-enter\n\n    /* .slide-fade-leave-active for below version 2.1.8 */\n {\n    transform: translateX(390px);\n  }\n  .wechat-login {\n    position: absolute;\n    width: 100%;\n    left: 0;\n    bottom: 110px;\n    img {\n      position: relative;\n      margin: auto;\n      display: block;\n      width: 70px;\n      height: 70px;\n      margin-top: 40px;\n    }\n    .wechat-tip {\n      position: relative;\n      margin: auto;\n      width: 400px;\n      height: 1px;\n      background: #c3c3c3;\n      &::after {\n        position: absolute;\n        left: 50%;\n        top: -13px;\n        transform: translate(-50%);\n        content: '其他登录方式';\n        width: auto;\n        height: 24px;\n        font-size: 24px;\n        color: #333333;\n        background: #fff;\n        padding: 0 10px;\n      }\n    }\n  }\n}\n</style>"]}]}