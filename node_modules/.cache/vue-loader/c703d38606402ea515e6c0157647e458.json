;\n  box-sizing: border-box;\n  border-bottom: 20px solid #f6f6f6;\n  padding: 24px 30px;\n}\n.msg-favour-box {\n  padding-left: 40px;\n  overflow: hidden;\n  width: 690px;\n  height: 120px;\n  border-radius: 10px;\n  background: #365cf6;\n  text-align: center;\n  line-height: 120px;\n  font-size: 26px;\n  color: #fff;\n  box-sizing: border-box;\n  padding-right: 60px;\n  .pop {\n    float: right;\n  }\n  .icon-shoucang2 {\n    float: left;\n    font-size: 50px;\n    margin-right: 20px;\n  }\n  .icon-shang {\n    float: right;\n    font-size: 36px;\n    margin-left: 20px;\n    color: #ffe554;\n  }\n  .total {\n    float: left;\n  }\n}\n",{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6GA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/module/message/favour","sourcesContent":["<template>\n  <Wrapper :paddingTop=\"paddingTop\" @scrolling=\"nextPage\">\n    <AppHeader ref=\"header\" title=\"对我感兴趣\"></AppHeader>\n    <!-- 顶部bar -->\n    <div class=\"msg-favour-topbox\">\n      <div class=\"msg-favour-box\"><i class=\"iconfont icon-shoucang2\"></i><span class=\"total\">总人数 {{pop}}</span></div>\n    </div>\n    <!-- 列表数据 -->\n    <template>\n      <div class=\"msg-favour-list\" v-for=\"(item,index) in lists\" :key=\"index\" @click=\"toPage(item.user_id)\">\n        <img class=\"avatar\" :src=\"item.user_id?(item.head_picture||item.head_portrait):'http://weixin.yuhongrocky.top/static/logo.png'\" />\n        <span class=\"name\">{{item.user_id?(item.user_name||item.wechat_name):'游客'+(item.name==null?'':'-'+item.name)}}<br /><span v-if=\"!item.user_id\" style=\"font-weight:normal;\">{{item.phone||''}}</span></span>\n        <span class=\"time\">{{item.created_at|timeToNow}}</span>\n      </div>\n      <div class=\"msg-favour-nonepic\" v-show=\"lists.length==0&&!firstIn\">\n        <img class=\"pic\" src=\"./lack.png\" />\n      </div>\n      <p class=\"msg-favour-tips\">{{loading?'正在加载...':lists.length > 8?'暂无更多':''}}</p>\n    </template>\n  </Wrapper>\n</template>\n<script>\nimport dialogService from '../../../service/dialogService'\nimport messageApi from '../../../api/message/index'\nimport AppLoading from '../../../components/AppLoading/index'\nimport AppHeader from '../../../components/AppHeader'\nimport Wrapper from '../../../components/Wrapper/index'\nimport { mapState } from 'vuex'\nexport default {\n  name: 'message-favour',\n  data() {\n    return {\n      transforms: [], //滑动删除管理\n      firstIn: true,\n      loading: false, // 是否在加载\n      windowHeight: null, // 可见区域高度\n      last_page: null, // 总页数\n      page: 1, // 当前页数\n      lists: [],\n      pop: '--' // 人气\n    }\n  },\n  components: {\n    AppLoading,\n    AppHeader,\n    Wrapper\n  },\n  async mounted() {\n    this.getlist()\n  },\n  computed: {\n    ...mapState({\n      platform: state => state.system.platform\n    }),\n    paddingTop: function() {\n      return this.platform === 'wechat' ? '0px' : '100px'\n    }\n  },\n  methods: {\n    // 获取列表\n    async getlist(load) {\n      this.loading = true\n      if (load) this.page++\n      let res = await messageApi.getFovourlist(this.page)\n      if (res.status !== 0) {\n        dialogService.alert(res.message)\n        return\n      }\n      if (load) {\n        this.lists = this.lists.concat(res.data.list.data)\n      } else {\n        this.pop = res.data.all\n        this.lists = res.data.list.data\n        this.firstIn = false\n      }\n      this.last_page = res.data.list.last_page\n      this.windowHeight = document.documentElement.clientHeight\n      this.loading = false\n    },\n    //下一页\n    nextPage(scrolled, scrollHeight) {\n      let scrollBottom = this.windowHeight + scrolled > scrollHeight - 50\n      if (!scrollBottom || this.loading || this.page >= this.last_page) {\n        return false\n      } else {\n        this.getlist(true)\n      }\n    },\n    // 去微网\n    async toPage(id) {\n      if (!id) {\n        dialogService.toast('游客没有微网')\n        return\n      }\n      let res = await messageApi.getTemp(id)\n      if (res.status !== 0) {\n        dialogService.alert(res.message)\n      } else {\n        if (res.data.id) {\n          this.$router.push(`/webPage/detail/${res.data.id}`)\n        } else {\n          dialogService.toast('该用户还没有个人微网')\n        }\n      }\n    }\n  }\n}\n</script>\n<style lang=\"scss\" scoped>\n.msg-favour-tips {\n  font-size: 28px;\n  text-align: center;\n  margin-top: 40px;\n  padding-bottom: 20px;\n}\n.msg-favour-list {\n  align-items: center;\n  width: 100%;\n  border-bottom: 1px solid #f5f5f5;\n  height: 150px;\n  box-sizing: border-box;\n  padding: 0 30px;\n  line-height: 50px;\n  display: flex;\n  .avatar {\n    width: 90px;\n    height: 90px;\n    border-radius: 50%;\n    margin-right: 25px;\n  }\n  .name {\n    flex: 1;\n    color: #000;\n    font-size: 30px;\n  }\n  .time {\n    font-size: 22px;\n    color: #666;\n  }\n}\n.msg-favour-nonepic {\n  text-align: center;\n  padding-top: 100px;\n  .pic {\n    width: 496px;\n    height: 411px;\n  }\n}\n.msg-favour-topbox {\n  height: 195px;\n  width: 100%;\n  box-sizing: border-box;\n  border-bottom: 20px solid #f6f6f6;\n  padding: 24px 30px;\n}\n.msg-favour-box {\n  padding-left: 40px;\n  overflow: hidden;\n  width: 690px;\n  height: 120px;\n  border-radius: 10px;\n  background: #365cf6;\n  text-align: center;\n  line-height: 120px;\n  font-size: 26px;\n  color: #fff;\n  box-sizing: border-box;\n  padding-right: 60px;\n  .pop {\n    float: right;\n  }\n  .icon-shoucang2 {\n    float: left;\n    font-size: 50px;\n    margin-right: 20px;\n  }\n  .icon-shang {\n    float: right;\n    font-size: 36px;\n    margin-left: 20px;\n    color: #ffe554;\n  }\n  .total {\n    float: left;\n  }\n}\n</style>"]}]}