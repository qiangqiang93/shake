  case 'circle':\n          platform != 'wechat'\n            ? androidJS.toElectView()\n            : this.$router.push('/circle/index')\n          break\n        default:\n          break\n      }\n    }\n  },\n  computed: {\n    ...mapState(['system'])\n  }\n}\n</script>\n<style lang=\"scss\" scoped>\n.activity-marketing {\n  position: relative;\n}\n.activity-marketing-img {\n  width: 100%;\n}\n.activity-marketing-item {\n  left: 50%;\n  position: absolute;\n  transform: translateX(-50%);\n  -webkit-transform: translateX(-50%);\n}\n.activity-marketing-card {\n  top: 950px;\n  width: 226px;\n  height: 63px;\n  border-radius: 32px;\n}\n.activity-marketing-webpage {\n  top: 1796px;\n  width: 684px;\n  height: 102px;\n  border-radius: 51px;\n}\n.activity-marketing-library {\n  top: 2370px;\n  width: 230px;\n  height: 60px;\n  border-radius: 30px;\n}\n.activity-marketing-circle {\n  width: 685px;\n  height: 102px;\n  bottom: 100px;\n  border-radius: 51px;\n}\n</style>"]}]}