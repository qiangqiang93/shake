m = this.windowHeight + scrolled > scrollHeight - 50\n      if (!scrollBottom || this.loading || this.page >= this.last_page) {\n        return false\n      } else {\n        this.getlist(true)\n      }\n    }\n  }\n}\n</script>\n<style lang=\"scss\" scoped>\n.warpper {\n  .tips {\n    font-size: 28px;\n    text-align: center;\n    margin-top: 40px;\n    padding-bottom: 20px;\n  }\n  .message {\n    .box {\n      .image {\n        width: 100%;\n        height: 360px;\n        object-fit: cover;\n        display: block;\n      }\n      .consume {\n        color: #000000;\n        font-size: 32px;\n        height: 140px;\n        line-height: 40px;\n        background: #fff;\n        text-align: left;\n        box-sizing: border-box;\n        padding: 30px;\n      }\n      box-shadow: 0px 2px 20px 0px rgba(0, 0, 0, 0.1);\n      width: 100%;\n      height: 500px;\n      border-radius: 10px;\n      overflow: hidden;\n    }\n    .time {\n      color: #666666;\n      font-size: 22px;\n      margin-bottom: 30px;\n    }\n    &:last-child {\n      margin-bottom: 50px;\n    }\n    width: 100%;\n    margin-top: 35px;\n    padding: 0 30px;\n    box-sizing: border-box;\n    text-align: center;\n  }\n  color: #000;\n  width: 100%;\n  box-sizing: border-box;\n  min-height: 100vh;\n  background: #f4f4f4;\n  overflow: hidden;\n  position: relative;\n}\n</style>\n \n "]}]}