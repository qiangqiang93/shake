this.webPage.firstIn = false\n        if (res.status !== 0) throw new Error(res.message)\n        this.webPage.loading = false\n        this.webPage.page = res.data.current_page\n        this.webPage.allPage = res.data.last_page\n        this.webPage.data = this.webPage.data.concat(res.data.data)\n      } catch (err) {\n        dialogService.alert(err.message)\n      }\n    },\n    // 获取营销网页\n    async getMarketing() {\n      try {\n        this.marketing.loading = true\n        let res = await marketing.getMarketingsByUserId(\n          this.userId,\n          this.marketing.page\n        )\n        if (this.marketing.firstIn) this.marketing.firstIn = false\n        if (res.status !== 0) throw new Error(res.message)\n        this.marketing.loading = false\n        this.marketing.page = res.data.current_page\n        this.marketing.allPage = res.data.last_page\n        this.marketing.data = this.marketing.data.concat(res.data.data)\n      } catch (err) {\n        dialogService.alert(err.message)\n      }\n    },\n    // 点击分享\n    shareHandler() {\n      shareService()\n    },\n    // 切换列表\n    toggleList(input) {\n      this.isWebPage = input\n    },\n    // 滚动处理函数\n    scrollHandler(scrolled, maxScroll) {\n      if (maxScroll - scrolled - this.windowHeight <= 20) {\n        if (this.isWebPage) {\n          if (this.webPage.loading) return\n          if (this.webPage.page === this.webPage.allPage) return\n          this.webPage.page = this.webPage.page + 1\n          this.getWebPage()\n        } else {\n          if (this.marketing.loading) return\n          if (this.marketing.page === this.marketing.allPage) return\n          this.marketing.page = this.marketing.page + 1\n          this.getMarketing()\n        }\n      }\n    }\n  },\n  computed: {\n    ...mapState({\n      platform: state => state.system.platform\n    }),\n    paddingTop: function() {\n      return this.platform === 'wechat' ? '0px' : '90px'\n    }\n  },\n  components: {\n    Wrapper,\n    AppLoading,\n    PreviewItem,\n    AppHeader\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.preview-wrapper {\n  position: relative;\n  background: #fff;\n}\n.raiders-for-app {\n  right: 30px;\n  color: #fff;\n  padding-left: 30px;\n  position: absolute;\n  .icon-fenxiang {\n    font-size: 32px;\n  }\n}\n.preview-header {\n  width: 100%;\n  height: 440px;\n  overflow: hidden;\n  position: relative;\n  background-size: 100%;\n  background-position: top;\n  background-repeat: no-repeat;\n  background-image: url('./headBG.gif');\n}\n.base-info {\n  width: 690px;\n  height: 210px;\n  padding: 40px;\n  display: flex;\n  overflow: hidden;\n  position: relative;\n  background: #fff;\n  border-radius: 10px;\n  margin: 178px auto 0;\n  box-sizing: border-box;\n  box-shadow: 0px 0px 25px 0px rgba(0, 0, 0, 0.1);\n}\n.base-info-head {\n  width: 100px;\n  height: 130px;\n  display: flex;\n  overflow: hidden;\n  align-items: center;\n  margin-right: 24px;\n  .head-img {\n    width: 100px;\n    height: 100px;\n    border-radius: 50px;\n  }\n}\n.base-info-content {\n  flex: 1;\n  display: flex;\n  font-size: 22px;\n  color: #454545;\n  overflow: hidden;\n  white-space: nowrap;\n  flex-direction: column;\n  text-overflow: ellipsis;\n  justify-content: space-between;\n  font-family: 'PingFang SC Medium';\n  .name {\n    font-size: 30px;\n    color: #000000;\n    font-family: 'PingFang SC Bold';\n  }\n  .cut-line {\n    margin: 0 20px;\n  }\n}\n.share-icon {\n  top: 40px;\n  right: 30px;\n  color: #454545;\n  position: absolute;\n  .icon-fenxiang {\n    font-size: 32px;\n  }\n}\n.preview-tabs {\n  width: 100%;\n  height: 60px;\n  padding: 0 30px;\n  font-size: 32px;\n  color: #999999;\n  overflow: hidden;\n  line-height: 36px;\n  position: relative;\n  box-sizing: border-box;\n  font-family: 'PingFang SC Bold';\n  .slide {\n    bottom: 0;\n    width: 60px;\n    height: 6px;\n    left: 62.5px;\n    position: absolute;\n    background: #274ff3;\n    transition: all ease-in-out 0.3s;\n   