ed, maxScroll) {\n      if (maxScroll - scrolled - this.windowHeight <= 20) {\n        if (this.isWebPage) {\n          if (this.webPage.loading) return\n          if (this.webPage.page === this.webPage.allPage) return\n          this.webPage.page = this.webPage.page + 1\n          this.getWebPage()\n        } else {\n          if (this.marketing.loading) return\n          if (this.marketing.page === this.marketing.allPage) return\n          this.marketing.page = this.marketing.page + 1\n          this.getMarketing()\n        }\n      }\n    }\n  },\n  computed: {\n    ...mapState({\n      platform: state => state.system.platform\n    }),\n    paddingTop: function() {\n      return this.platform === 'wechat' ? '0px' : '90px'\n    }\n  },\n  components: {\n    Wrapper,\n    AppLoading,\n    PreviewItem,\n    AppHeader\n  }\n}\n",{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuDA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/module/other/preview","sourcesContent":["<template>\n  <Wrapper class=\"preview-wrapper\" :paddingTop=\"paddingTop\" @scrolling=\"scrollHandler\">\n    <AppHeader ref=\"header\" :title=\"cardInfo.name\" iconColor=\"#fff\">\n    </AppHeader>\n    <div class=\"preview-header\">\n      <div class=\"base-info\">\n        <div class=\"base-info-head\">\n          <img :src=\"cardInfo.portrait\" alt=\"\" class=\"head-img\">\n        </div>\n        <div class=\"base-info-content\">\n          <p class=\"name\" v-text=\"cardInfo.name\"></p>\n          <p class=\"works\">\n            <span v-text=\"cardInfo.profession\"></span>\n            <span class=\"cut-line\">|</span>\n            <span v-text=\"cardInfo.industry\"></span>\n          </p>\n          <p class=\"company\" v-text=\"cardInfo.company\"></p>\n        </div>\n        <div class=\"share-icon\" @click=\"shareHandler()\">\n          <i class=\"iconfont icon-fenxiang\"></i>\n        </div>\n      </div>\n    </div>\n    <div ref=\"tabs\" class=\"preview-tabs\">\n      <p class=\"preview-tab\" :class=\"{'active': isWebPage}\" @click=\"toggleList(true)\">个人微网</p>\n      <p class=\"preview-tab\" :class=\"{'active': !isWebPage}\" @click=\"toggleList(false)\">营销网页</p>\n      <p class=\"slide\" :class=\"{'active': !isWebPage}\"></p>\n    </div>\n    <div class=\"preview-list\">\n      <template v-if=\"isWebPage\">\n        <AppLoading v-if=\"webPage.firstIn\" visible=\"true\"></AppLoading>\n        <router-link v-for=\"(item, index) in webPage.data\" :key=\"index\" :to=\"'/webPage/detail/' + item.id\">\n          <PreviewItem :item=\"item\"></PreviewItem>\n        </router-link>\n        <div v-if=\"!webPage.data.length\" class=\"empty-img-container\">\n          <img src=\"./empty.jpg\" alt=\"\" class=\"empty-img\">\n          TA还没有制作个人微网\n        </div>\n      </template>\n      <template v-else>\n        <AppLoading v-if=\"marketing.firstIn\" visible=\"true\"></AppLoading>\n        <router-link v-for=\"(item, index) in marketing.data\" :key=\"index\" :to=\"'/marketing/detail/' + item.id\">\n          <PreviewItem :item=\"item\"></PreviewItem>\n        </router-link>\n        <div v-if=\"!marketing.data.length\" class=\"empty-img-container\">\n          <img src=\"./empty.jpg\" alt=\"\" class=\"empty-img\">\n          TA还没有制作营销网页\n        </div>\n      </template>\n   