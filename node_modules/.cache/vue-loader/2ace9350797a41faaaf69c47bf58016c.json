: 100%;\n  min-height: 100vh;\n  background: #f6f6f6;\n  overflow: hidden;\n  position: relative;\n  .tips {\n    color: #666666;\n    font-size: 22px;\n    margin-top: 30px;\n    width: 100%;\n    box-sizing: border-box;\n    padding-left: 30px;\n  }\n  /deep/ .icon-chongzhijiaofei {\n    color: #187af4;\n    font-size: 40px;\n  }\n  /deep/ .icon-tixian {\n    color: #f8821a;\n    font-size: 40px;\n  }\n  .wall {\n    width: 100%;\n    height: 310px;\n    box-sizing: border-box;\n    padding: 0 30px;\n    background: #2e53eb;\n    overflow: hidden;\n    position: relative;\n    color: #fff;\n    .wall-detail {\n      font-size: 26px;\n      position: absolute;\n      top: 20px;\n      right: 30px;\n    }\n    .wall-title {\n      font-size: 26px;\n      margin: 95px 0 30px;\n    }\n    .wall-price {\n      font-size: 72px;\n      font-weight: bold;\n    }\n  }\n}\n</style>\n \n "]}]}