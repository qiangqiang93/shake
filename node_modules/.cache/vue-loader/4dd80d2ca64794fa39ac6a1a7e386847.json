dex\n      this.rightIndustryIndex = null\n    },\n    checkRight: function(index) {\n      this.rightIndustryIndex = index\n      eventBus.$emit(\n        'checkIndustry',\n        this.rightIndustry[this.leftIndustryIndex][index].text\n      )\n      this.$router.back()\n    }\n  }\n}\n</script>\n<style lang=\"scss\" scoped>\n.leftContain {\n  height: 100vh;\n  overflow: auto;\n  width: 300px;\n  float: left;\n}\n\n.leftContain::-webkit-scrollbar {\n  width: 0;\n  height: 0;\n  color: transparent;\n}\n\n.left {\n  width: 300px;\n}\n.left .leftItem {\n  width: 252px;\n  padding-bottom: 10px;\n  font-size: 32px;\n  line-height: 60px;\n  padding-left: 24px;\n  padding-right: 24px;\n  font-family: PingFang SC;\n  background: whitesmoke;\n  padding: 20px;\n  box-sizing: border-box;\n}\n\n.right {\n  float: left;\n  width: 450px;\n}\n.right .rightItem {\n  width: 402px;\n  padding-bottom: 10px;\n  color: #535257;\n  font-size: 28px;\n  line-height: 60px;\n  padding-left: 24px;\n  padding-right: 24px;\n  font-family: PingFang SC;\n  background: white;\n}\n\n.active {\n  background: white !important;\n}\n\n.rightActive:hover {\n  background: whitesmoke !important;\n}\n</style>\n"]}]}