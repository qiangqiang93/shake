 该项数据\n\n-->\n<template>\n  <div class=\"preview-item\">\n    <template v-if=\"isWebPage\">\n      <div class=\"preview-item-poster\" :style=\"poster\">\n      </div>\n      <div class=\"preview-item-content\">\n        <p class=\"preview-item-name\" v-text=\"item.title\"></p>\n        <p class=\"preview-item-visited\" v-text=\"'浏览量' + item.popularity\"></p>\n        <p class=\"preview-item-price\" v-text=\"item.clone_price === '0.00' ? '免费' : item.clone_price + '元'\"></p>\n      </div>\n    </template>\n    <template v-else>\n      <div class=\"preview-item-poster\" :style=\"poster\">\n      </div>\n      <div class=\"preview-item-content\">\n        <p class=\"preview-item-name\" v-text=\"item.title\">名称</p>\n        <p class=\"preview-item-visited\" v-text=\"'浏览量' + item.popularity\"></p>\n      </div>\n    </template>\n  </div>\n</template>\n\n<script>\nimport { pageCover, getMarketingCover } from '../../utils'\nexport default {\n  props: ['item'],\n  data() {\n    return {}\n  },\n  mounted() {},\n  computed: {\n    isWebPage: function() {\n      // true[个人微网]、false[营销网页]\n      return this.item.clone_price !== undefined\n    },\n    poster: function() {\n      if (this.item.clone_price !== undefined) {\n        this.item.type = 2\n        return {\n          'background-image': 'url(' + pageCover(this.item) + ')'\n        }\n      } else {\n        let contents = JSON.parse(this.item.contents)\n        return {\n          'background-image': 'url(' + getMarketingCover(contents.content) + ')'\n        }\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.preview-item {\n  width: 750px;\n  height: 200px;\n  display: flex;\n  padding: 15px 30px;\n  box-sizing: border-box;\n}\n.preview-item-poster {\n  width: 300px;\n  height: 170px;\n  overflow: hidden;\n  margin-right: 35px;\n  border-radius: 5px;\n  background-size: cover;\n  background-position: center;\n}\n.preview-item-content {\n  flex: 1;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-around;\n}\n.preview-item-name {\n  font-size: 28px;\n  overflow: hidden;\n  color: #111111;\n  line-height: 42px;\n  display: -webkit-box;\n  -webkit-line-clamp: 2;\n  -webkit-box-orient: vertical;\n  font-family: 'PingFang SC Medium';\n}\n.preview-item-visited,\n.preview-item-price {\n  height: 24px;\n  font-size: 24px;\n  color: #999999;\n  line-height: 24px;\n  font-family: 'PingFang SC Medium';\n  &.active {\n    color: #fac216;\n  }\n}\n</style>\n"]}]}