oading) document.querySelector('#circleAudio').play()\n      } finally {\n        this.list = moak\n        this.loading = false\n      }\n    }\n  },\n  components: {\n    CircleBall,\n    CircleMeteor\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.circle-wrapper {\n  height: 100vh;\n  padding: 100px 0;\n  background: #181a23;\n}\n.circle-container {\n  height: 100%;\n  display: flex;\n  overflow: hidden;\n  position: relative;\n  flex-direction: column;\n  justify-content: space-around;\n  canvas {\n    position: absolute;\n  }\n}\n.circle-ball {\n  width: 660px;\n  height: 660px;\n  overflow: hidden;\n  margin: 0px auto;\n}\n.circle-tips-box {\n  color: #fff;\n  overflow: hidden;\n  text-align: center;\n  position: relative;\n  font-family: 'PingFang SC Medium';\n  .circle-small-tips {\n    font-size: 22px;\n    color: #d5d5d5;\n  }\n  .circle-desc-tips {\n    font-size: 24px;\n    margin-top: 20px;\n    color: #ffffff;\n  }\n}\n</style>\n"]}]}