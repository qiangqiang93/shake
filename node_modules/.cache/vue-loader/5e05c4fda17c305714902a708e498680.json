eTask: state => state.circle.shareTask\n    }),\n    paddingTop: function() {\n      return this.platform !== 'wechat' ? '180px' : '100px'\n    }\n  },\n  components: {\n    Wrapper,\n    Loading,\n    CircleItem,\n    AndroidHeader\n  }\n}\n",{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;AA0BA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/module/circle/task","sourcesContent":["<template>\n  <Wrapper class=\"circle-task\" :paddingTop=\"paddingTop\">\n    <div class=\"circle-task-header\">\n      <AndroidHeader title=\"分享记录\" iconColor=\"#fff\" fontColor=\"#fff\" background=\"#274ff3\"></AndroidHeader>\n      <div class=\"circle-task-tips\" v-if=\"showTips\">\n        <p class=\"task-tips-text\">\n          他人参加互推分享了您，您必须分享完，才能使用互推圈功能，才能看到他人为您带来的浏览量！\n        </p>\n        <p class=\"task-tips-icon\" @click=\"closeTips()\">\n          <img class=\"tips-icon-img\" src=\"./close.png\" alt=\"\">\n        </p>\n      </div>\n    </div>\n    <Loading v-if=\"loading\" visible=\"true\"></Loading>\n    <div v-else class=\"circle-task-list\">\n      <template v-if=\"shareTask.length\">\n        <CircleItem v-for=\"(item, index) in shareTask\" :key=\"index\" :item=\"item\" type=\"task\" class=\"circle-item-wrapper\"></CircleItem>\n      </template>\n      <template v-else>\n        <img src=\"./empty.png\" alt=\"\" class=\"empty-task\">\n      </template>\n    </div>\n  </Wrapper>\n</template>\n\n<script>\nimport circle from '../../../api/circle'\nimport { mapState, mapActions } from 'vuex'\nimport Wrapper from '../../../components/Wrapper'\nimport Loading from '../../../components/Loading'\nimport CircleItem from '../../../components/CircleItem'\nimport AndroidHeader from '../../../components/AndroidHeader'\nimport dialogService from '../../../service/dialogService'\nexport default {\n  data() {\n    return {\n      loading: true, // 第一次加载\n      showTips: true // 是否显示提示\n    }\n  },\n  mounted() {\n    this.getShareTask()\n  },\n  methods: {\n    // 获取分享任务\n    async getShareTask() {\n      try {\n        let res = await circle.getCircleList(3)\n        if (res.status !== 0) throw new Error(res.message)\n        this.loading = false\n      } catch (err) {\n        dialogService.alert(err.message)\n      }\n    },\n    // closeTips\n    closeTips() {\n      this.showTips = false\n      this.paddingTop = this.platform !== 'wechat' ? '80px' : '0px'\n    }\n  },\n  computed: {\n    ...mapState({\n      user: state => state.user.userInfo,\n      platform: state => state.system.platform,\n      shareTask: state => state.circle.shareTask\n    }),\n    paddingTop: function() {\n      return this.platform !== 'wechat' ? '180px' : '100px'\n    }\n  },\n  components: {\n    Wrapper,\n    Loading,\n    CircleItem,\n    AndroidHeader\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.circle-task {\n  transition: all ease 0.4s;\n}\n.circle-task-header {\n  top: 0;\n  left: 0;\n  width: 100%;\n  position: fixed;\n  overflow: hidden;\n  background: #fff;\n  .android-header {\n    position: relative;\n  }\n}\n.circle-task-tips {\n  height: 100px;\n  display: flex;\n  font-size: 22px;\n  color: #ffffff;\n  padding: 14px 30px;\n  background: #09cc9c;\n  box-sizing: border-box;\n  justify-content: space-around;\n  font-family: 'PingFang SC Medium';\n  border-bottom: 1px solid #e1e1e1;\n  .task-tips-text {\n    flex: 1;\n    line-height: 36px;\n  }\n  .task-tips-icon {\n    width: 60px;\n    display: flex;\n    align-items: center;\n    justify-content: flex-end;\n  }\n  .tips-icon-img {\n    width: 30px;\n    height: 30px;\n  }\n}\n.circle-task-list {\n  overflow: hidden;\n  margin-top: 40px;\n}\n.empty-task {\n  width: 360px;\n  display: block;\n  margin: 40px auto;\n}\n\n.circle-item-wrapper {\n  margin: 0 auto 30px;\n}\n</style>\n"]}]}