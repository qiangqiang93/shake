 v-show=\"platform !== 'wechat'\">\n    <span class=\"iconfont icon-fanhui\" @click=\"back()\"></span>\n    <p class=\"app-header-title\" v-text=\"title\"></p>\n    <slot></slot>\n  </div>\n</template>\n\n<script>\nimport { mapState } from 'vuex'\nexport default {\n  props: ['title', 'iconColor', 'fontColor', 'background', 'beforeBack'],\n  data() {\n    return {}\n  },\n  mounted() {},\n  methods: {\n    back() {\n      if (this.beforeBack) {\n        let shouldBack = this.beforeBack()\n        if (!shouldBack) return false\n      }\n      if (this.platform !== 'wechat') {\n        try {\n          androidJS.toBackView()\n        } catch {\n          this.$router.back()\n        }\n      } else {\n        this.$router.back()\n      }\n    }\n  },\n  computed: {\n    ...mapState({\n      platform: state => state.system.platform\n    })\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.app-header {\n  top: 0;\n  width: 100%;\n  z-index: 999;\n  height: 90px;\n  position: fixed;\n  line-height: 90px;\n  text-align: center;\n  background: #fff;\n  border-bottom: 1px solid #e0e0e0;\n}\n.icon-fanhui {\n  left: 30px;\n  color: #333;\n  font-size: 36px;\n  position: absolute;\n}\n.app-header-title {\n  color: #333;\n  font-size: 32px;\n  font-family: 'PingFang SC Bold';\n}\n</style>\n"]}]}