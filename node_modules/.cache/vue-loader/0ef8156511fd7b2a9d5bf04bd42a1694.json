n-jiufuqianbaoicon14\"></i>\n        </li>\n      </ul>\n    </div>\n  </div>\n</template>\n<script>\nimport setting from '../../../api/setting'\nimport user from '../../../api/user'\nimport AppHeader from '../../../components/AppHeader'\nimport dialogService from '../../../service/dialogService'\nimport { mapState } from 'vuex'\nexport default {\n  name: 'mine-CardUnbind',\n  components: { AppHeader },\n  data() {\n    return {\n      value: '' // 支付密码\n    }\n  },\n  computed: {\n    ...mapState({\n      platform: state => state.system.platform\n    }),\n    paddingTop: function() {\n      return this.platform === 'wechat' ? '0px' : '100px'\n    }\n  },\n  methods: {\n    focus() {\n      this.$refs.pwd.focus()\n    },\n    inputHandler() {\n      if (this.value.length === 6) {\n        this.validatePwd()\n      }\n    },\n    // 删除银行卡\n    async deleBank() {\n      let res = await user.delBank(this.$route.query.id)\n      if (res.status !== 0) {\n        dialogService.alert(res.message)\n        return\n      }\n      dialogService.alert('解绑成功')\n      this.$router.go(-2)\n    },\n    async validatePwd() {\n      try {\n        let res = await setting.validatePymPwd(this.value)\n        if (res.status !== 0) {\n          dialogService.alert(res.message)\n          return\n        }\n        this.deleBank()\n      } catch (error) {\n        dialogService.toast(error.message)\n      }\n    }\n  }\n}\n</script>\n<style lang=\"scss\" scoped>\n.warpper {\n  .revise-pay-pwd {\n    width: 100%;\n    height: 100vh;\n    background: #f6f6f6;\n    .title {\n      padding: 44px 30px;\n      font-size: 26px;\n      color: #555555;\n      box-sizing: border-box;\n    }\n    .input {\n      left: 5%;\n      top: 0;\n      width: 0;\n      opacity: 0;\n      position: absolute;\n      height: 0;\n      outline: none;\n    }\n    .pwd-wrap {\n      width: 90%;\n      display: flex;\n      margin: 0 auto;\n      padding-bottom: 1px;\n      justify-content: space-between;\n      li {\n        width: 100px;\n        height: 100px;\n        line-height: 100px;\n        text-align: center;\n        border: 1px solid #ddd;\n        background: #fff;\n        i {\n          font-size: 26px;\n        }\n      }\n    }\n  }\n  .card {\n    text-align: center;\n    .card-name {\n      font-size: 38px;\n      margin-top: 165px;\n    }\n    .card-no {\n      font-size: 24px;\n      color: #333333;\n      margin: 35px 0 95px;\n    }\n  }\n  color: #000;\n  width: 100%;\n  box-sizing: border-box;\n  padding: 0 30px;\n  min-height: 100vh;\n  background: #f5f5f5;\n  overflow: hidden;\n  position: relative;\n}\n</style>\n \n "]}]}