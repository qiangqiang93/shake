    this.getWebPage()\n          break\n        case 3:\n          this.getMarketing()\n          break\n        case 4:\n          this.getCard()\n          break\n        default:\n          dialogService.alert('不合法的参数')\n        // nothing\n      }\n    },\n    // 获取名片详情\n    async getCard() {\n      try {\n        let res = await card.getCardByUserId(this.materialId)\n        if (res.status !== 0) throw new Error(res.message)\n        this.material = res.data\n        this.loading = false\n      } catch (err) {\n        dialogService.alert(err.message)\n      }\n    },\n    // 获取文章详情\n    async getArticle() {\n      try {\n        let res = await article.getArticleById(this.materialId)\n        if (res.status !== 0) throw new Error(res.message)\n        this.material = res.data\n        this.loading = false\n      } catch (err) {\n        dialogService.alert(err.message)\n      }\n    },\n    // 获取个人微网\n    async getWebPage() {\n      try {\n        let res = await webPage.getWebPageById(this.materialId)\n        if (res.status !== 0) throw new Error(res.message)\n        res.data.content = JSON.parse(res.data.content)\n        this.material = res.data\n        this.loading = false\n      } catch (err) {\n        dialogService.alert(err.message)\n      }\n    },\n    // 获取营销网页\n    async getMarketing() {\n      try {\n        let res = await marketing.getMarketingDetail(this.materialId)\n        if (res.status !== 0) throw new Error(res.message)\n        this.material = JSON.parse(res.data.contents)\n        this.loading = false\n      } catch (err) {\n        dialogService.alert(err.message)\n      }\n    }\n  },\n  components: {\n    Card,\n    Loading,\n    CustomComponent\n  }\n}\n",{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiDA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;