      <div class=\"spread-shared\">\n        <span class=\"iconfont icon-share1\"></span>\n        <span class=\"count\">0</span>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { format } from 'date-fns'\nimport { mapActions, mapState } from 'vuex'\nimport SlideShow from '../../../components/SlideShow'\nexport default {\n  name: 'spreadPreview',\n  data() {\n    return {\n      index: 0,\n      graphics: [], // 图文内容副本\n      date: format(new Date(), 'YYYY/MM/DD')\n    }\n  },\n  beforeMount() {},\n  mounted() {\n    try {\n      this.graphics = JSON.parse(this.myAD.content)\n    } catch (err) {\n      this.graphics = []\n    }\n  },\n  methods: {\n    goToCard() {\n      this.$router.push({\n        path: `/card/detail/${this.myAD.user_id}`\n      })\n    },\n    goMarketing() {\n      if (this.myAD.user_marketing_id) {\n        this.$router.push({\n          path: `/marketing/detail/${this.myAD.user_marketing_id}`\n        })\n      }\n    },\n    touchHandler(event, type) {\n      if (type === 'start') {\n        this.pos = event.touches[0].pageX\n      } else {\n        let pos = event.changedTouches[0].pageX\n        if (pos - this.pos >= 40) {\n          // 左滑\n          this.index =\n            this.index !== 0 ? (this.index = this.index - 1) : this.index\n        } else if (pos - this.pos <= -40) {\n          // 右滑\n          this.index =\n            this.index !== this.graphics.length - 1\n              ? (this.index = this.index + 1)\n              : this.index\n        }\n        this.pos = null\n      }\n    }\n  },\n  components: {\n    SlideShow\n  },\n  computed: {\n    ...mapState({\n      user: state => state.user.userInfo,\n      myAD: state => state.spread.spreadAD\n    })\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.spread-wrapper {\n  width: 100%;\n  height: 100vh;\n  overflow: hidden;\n  background: #1b1b1b;\n\n  .slider-wrapper {\n    width: 100%;\n    height: 100%;\n  }\n  .spread-info {\n    height: 50px;\n    font-size: 26px;\n    color: #999999;\n    margin-top: 30px;\n    line-height: 50px;\n    font-family: 'PingFang SC Medium';\n    .spread-date {\n      float: left;\n      margin-left: 20px;\n    }\n    .spread-coin {\n      float: right;\n      margin-right: 20px;\n    }\n  }\n  .spread-txt {\n    color: #fff;\n    font-size: 32px;\n    margin-top: 40px;\n    text-indent: 20px;\n    font-family: 'PingFang SC Medium';\n  }\n  .icon-chaolianjie {\n    color: #46a9ff;\n  }\n  .ad-info {\n    top: 320px;\n    right: 20px;\n    width: 80px;\n    color: #fff;\n    font-size: 24px;\n    position: absolute;\n    text-align: center;\n    font-family: 'PingFang SC Medium';\n    .spread-card {\n      width: 80px;\n      height: 80px;\n      overflow: hidden;\n      border-radius: 50%;\n    }\n    img {\n      width: 100%;\n      height: 100%;\n    }\n    .spread-visited,\n    .spread-shared {\n      display: flex;\n      margin-top: 50px;\n      align-items: center;\n      flex-direction: column;\n      justify-content: center;\n      text-shadow: 0px 2px 6px rgba(0, 0, 0, 0.2);\n    }\n    .icon-liulan {\n      font-size: 28px;\n    }\n    .icon-share1 {\n      font-size: 36px;\n    }\n    .count {\n      margin-top: 10px;\n    }\n  }\n  .spread-mask {\n    left: 0;\n    bottom: 0;\n    width: 100%;\n    position: absolute;\n    padding-bottom: 60px;\n    background: linear-gradient(0deg, rgba(27, 27, 27, 0.3), rgba(0, 0, 0, 0));\n  }\n}\n</style>\n"]}]}