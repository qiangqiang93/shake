in-top: 50px;\n\n    > div {\n      font-size: 32px;\n      color: #34394c;\n      padding: 0 24px 10px;\n      box-sizing: border-box;\n      border-bottom: 2px solid #f2f2f2;\n\n      span {\n        font-size: 24px;\n        color: #bababa;\n      }\n    }\n\n    ul {\n      width: 100%;\n      padding: 0 35px;\n      box-sizing: border-box;\n      overflow: hidden;\n      min-height: 300px;\n\n      li {\n        float: left;\n        width: 158px;\n        box-sizing: border-box;\n        height: 68px;\n        background: rgba(247, 247, 247, 1);\n        border: 1px solid rgba(233, 233, 233, 1);\n        border-radius: 52px;\n        line-height: 68px;\n        text-align: center;\n        font-size: 28px;\n        color: #676767;\n        margin: 24px 16px 0 0;\n        animation: zoomIn 0.3s ease;\n\n        &:nth-of-type(4n) {\n          margin-right: 0;\n        }\n      }\n    }\n\n    @keyframes zoomIn {\n      from {\n        opacity: 0;\n        -webkit-transform: scale3d(0.3, 0.3, 0.3);\n        transform: scale3d(0.3, 0.3, 0.3);\n      }\n\n      to {\n        opacity: 1;\n      }\n    }\n  }\n}\n",{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0HA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/module/mine/marketLabel","sourcesContent":["<template>\n  <div class=\"marketing-label\">\n    <AppHeader ref=\"header\" title=\"营销标签\"></AppHeader>\n    <div class=\"edit\">\n      <div class=\"btn\" @click=\"submit\">保存</div>\n    </div>\n    <p class=\"title title1\">选择营销标签 不错失潜在客户</p>\n    <p class=\"title title2\">利用文库推荐频道向客户分享精准文章</p>\n\n    <div class=\"select classify-item\">\n      <div>\n        我的标签&nbsp;&nbsp;&nbsp;<span>点击删除</span>\n      </div>\n      <ul>\n        <li v-for=\"(item,index) in set\" :key=\"item.id\" @click=\"delList(item,index)\">\n          {{item.name}}\n        </li>\n      </ul>\n    </div>\n    <div class=\"un-select classify-item\">\n      <div>\n        标签推荐&nbsp;&nbsp;&nbsp;<span>点击添加</span>\n      </div>\n      <ul>\n        <li v-for=\"(item ,index) in unset\" :key=\"item.id\" @click=\"addList(index)\">\n          {{item.name}}\n        </li>\n      </ul>\n    </div>\n    <loading v-if=\"isRequst\"></loading>\n  </div>\n</template>\n<script>\nimport dialogService from '../../../service/dialogService.js'\nimport AppHeader from '../../../components/AppHeader'\nimport User from '../../../api/user'\nimport loading from '../../../components/AppLoading'\nimport { mapState, mapActions } from 'vuex'\nexport default {\n  name: 'marketIndex',\n  data() {\n    return {\n      set: [], //已选择标签\n      unset: [], //未选择标签\n      isClick: true, // 是否可以点击\n      isRequst: true, //  是否正在加载\n      isSave: false, // 是否保存\n      defaultData: {}\n    }\n  },\n  mounted() {\n    this.init()\n  },\n  activated() {\n    if (this.isSave) this.isSave = false\n  },\n  components: {\n    AppHeader,\n    loading\n  },\n  methods: {\n    async init() {\n      try {\n        let res = await User.label()\n        if (res.status !== 0) {\n          throw new Error(res.message)\n        }\n        this.isRequst = false\n        this.defaultData = JSON.parse(JSON.stringify(res.data))\n        this.set = res.data.set\n        this.unset = res.data.unset\n      } catch (error) {\n        dialogService.toast(error.message)\n      }\n    },\n    // 删除列表\n    delList(item, index) {\n      this.unset.push(this.set[index])\n      this.set.splice(index, 1)\n    },\n    // 添加列表\n    addList(index) {\n      this.set.push(this.unset[index])\n      this.unset.splice(index, 1)\n    },\n    // 编辑完成\n    async submit() {\n      if (!this.isClick) return false\n      try {\n        let arr = this.set.map(item => {\n          return item.id\n        })\n        this.isClick = false\n        let res = await User.addLabel(arr.join(','))\n        if (res.status !== 0) {\n          throw new Error(res.message)\n        }\n\n        this.isClick = true\n        //保存后默认值为当前的\n        this.isSave = true\n        this.defaultData.set = JSON.parse(JSON.stringify(this.set))\n        this.defaultData.unset = JSON.parse(JSON.stringify(this.unset))\n        this.$router.go(-1)\n      } catch (error) {\n        dialogService.toast(error.message)\n      }\n    }\n  },\n  watch: {\n    $route(to, from) {\n      // 因为页面缓存，当为保存的时候应该回复之前\n      if (to.path.indexOf('/mine') > -1 && !this.isSave) {\n        this.set = JSON.parse(JSON.stringify(this.defaultData.set))\n        this.unset = JSON.parse(JSON.stringify(this.defaultData.unset))\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.marketing-label {\n  overflow: hidden;\n\n  .edit {\n    height: 72px;\n    display: flex;\n    padding: 0 30px;\n    box-sizing: border-box;\n    justify-content: flex-end;\n    align-items: center;\n\n    .btn {\n      position: relative;\n      width: 116px;\n      height: 50px;\n      border: 1px solid rgba(39, 79, 243, 1);\n      border-radius: 52px;\n      text-align: center;\n      line-height: 50px;\n      font-size: 28px;\n      color: #274ff3;\n    }\n  }\n\n  .title {\n    text-align: center;\n    font-size: 24px;\n    color: #676767;\n\n    &:nth-of-type(1) {\n      font-size: 36px;\n      color: #34394c;\n      margin: 68px 0 30px;\n    }\n  }\n\n  .classify-item {\n    margin-top: 50px;\n\n    > div {\n      font-size: 32px;\n      color: #34394c;\n      padding: 0 24px 10px;\n      box-sizing: border-box;\n      border-bottom: 2px solid #f2f2f2;\n\n      span {\n        font-size: 24px;\n        color: #bababa;\n      }\n    }\n\n    ul {\n      width: 100%;\n      padding: 0 35px;\n      box-sizing: border-box;\n      overflow: hidden;\n      min-height: 300px;\n\n      li {\n        float: left;\n        width: 158px;\n        box-sizing: border-box;\n        height: 68px;\n        background: rgba(247, 247, 247, 1);\n        border: 1px solid rgba(233, 233, 233, 1);\n        border-radius: 52px;\n        line-height: 68px;\n        text-align: center;\n        font-size: 28px;\n        color: #676767;\n        margin: 24px 16px 0 0;\n        animation: zoomIn 0.3s ease;\n\n        &:nth-of-type(4n) {\n          margin-right: 0;\n        }\n      }\n    }\n\n    @keyframes zoomIn {\n      from {\n        opacity: 0;\n        -webkit-transform: scale3d(0.3, 0.3, 0.3);\n        transform: scale3d(0.3, 0.3, 0.3);\n      }\n\n      to {\n        opacity: 1;\n      }\n    }\n  }\n}\n</style>"]}]}