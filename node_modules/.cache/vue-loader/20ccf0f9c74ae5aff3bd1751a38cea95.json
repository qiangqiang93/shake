 = 10\n      this.clone_price = val\n    },\n    cancel() {\n      this.$emit('closeHandle', '')\n    },\n    submit() {\n      this.$emit('closeHandle', {\n        is_clone: this.is_clone,\n        clone_price: this.clone_price\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.clone-dialog {\n  top: 0;\n  left: 0;\n  z-index: 2;\n  width: 100%;\n  height: 100%;\n  display: flex;\n  position: fixed;\n  align-items: center;\n  justify-content: center;\n  background: rgba(11, 11, 11, 0.64);\n}\n.clone-container {\n  width: 620px;\n  height: 540px;\n  display: flex;\n  overflow: hidden;\n  border-radius: 5px;\n  background: #ffffff;\n  box-sizing: border-box;\n  flex-direction: column;\n  transition: all ease 0.4s;\n  animation: show 0.4s ease;\n  .clone-title {\n    width: 100%;\n    height: 120px;\n    font-size: 34px;\n    color: #333333;\n    font-weight: bold;\n    line-height: 140px;\n    text-align: center;\n    font-family: PingFang SC;\n  }\n  .clone-main {\n    flex: 1;\n    width: 100%;\n    padding: 42px;\n    box-sizing: border-box;\n  }\n  .clone-radios {\n    display: flex;\n    color: #333;\n    font-size: 28px;\n    font-weight: 400;\n    flex-direction: column;\n    font-family: PingFang SC;\n    .clone-price {\n      justify-content: left;\n      width: 100%;\n      box-sizing: border-box;\n      margin-left: 66px;\n    }\n    .clone-unhope {\n      margin-bottom: 0;\n    }\n    span {\n      margin-bottom: 35px;\n    }\n  }\n  span.active {\n    i {\n      background: #274ff3;\n    }\n  }\n  i {\n    float: left;\n    width: 40px;\n    height: 40px;\n    box-sizing: border-box;\n    color: #fff;\n    border-radius: 50%;\n    display: block;\n    line-height: 36px;\n    text-align: center;\n    margin-top: -4px;\n    font-size: 24px;\n    transition: all 0.3s ease-in-out;\n    border: 2px solid #737373;\n    margin-right: 28px;\n    background: rgba(255, 255, 255, 1);\n    &::before {\n      scale: (0.8);\n    }\n  }\n  input {\n    width: 400px;\n    height: 70px;\n    outline: none;\n    margin-right: 20px;\n    border-bottom: 1px solid #adadad;\n  }\n}\n.clone-foot {\n  width: 100%;\n  height: 101px;\n  overflow: hidden;\n  border-top: 1px solid #eaeaea;\n  button {\n    float: left;\n    width: 50%;\n    height: 101px;\n    border: none;\n    outline: none;\n    display: block;\n    font-size: 34px;\n    font-family: PingFang SC;\n    font-weight: bold;\n    line-height: 50px;\n    background: rgba(255, 255, 255, 1);\n    padding: 0;\n  }\n  .clone-cancel {\n    border-right: 1px solid #eaeaea;\n    color: #666666;\n  }\n  .clone-submit {\n    color: #374ff3;\n  }\n}\n@keyframes show {\n  0% {\n    transform: scale(0);\n  }\n  100% {\n    transform: scale(1);\n  }\n}\n</style>\n\n"]}]}