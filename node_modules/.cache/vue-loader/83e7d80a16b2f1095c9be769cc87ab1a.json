to right, #385df9, #365cf8);\n  }\n  .iconfont {\n    position: relative;\n    width: 90px;\n    height: 90px;\n    display: block;\n    margin: 0 auto 16px;\n    line-height: 90px;\n    color: #fff;\n    border-radius: 50%;\n    font-size: 40px;\n  }\n  .reddot {\n    position: absolute;\n    display: block;\n    width: 20px;\n    height: 20px;\n    border-radius: 50%;\n    border: 3px solid #fff;\n    background: #ef2323;\n    top: 0;\n    right: 0;\n  }\n}\n",{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/module/message/index","sourcesContent":["<template>\n  <Wrapper paddingTop=\"0\" class=\"message-warpper\">\n    <AppHeader ref=\"header\" title=\"消息\" iconColor=\"#fff\" fontColor=\"#fff\" background=\"#000\"></AppHeader>\n    <!-- loading组件 -->\n    <AppLoading v-if=\"firstIn\"></AppLoading>\n    <!-- 顶部bar -->\n    <div class=\"message-topbar\">\n      <router-link class=\"message-topbar-item\" tag=\"div\" @click.native=\"popRed = false\" to=\"/message/popular\">\n        <i class=\"iconfont icon-woderenqi\">\n          <span v-show=\"popRed\" class=\"reddot\"></span>\n        </i>\n        <p>人气</p>\n      </router-link>\n      <div class=\"message-topbar-item\" @click=\"toFavour\">\n        <i class=\"iconfont icon-duiwoganxingqu\"></i>\n        <p>对我感兴趣</p>\n      </div>\n      <router-link class=\"message-topbar-item\" tag=\"div\" to=\"/message/system\">\n        <i class=\"iconfont icon-xitongxiaoxi\"></i>\n        <p>系统通知</p>\n      </router-link>\n    </div>\n    <!-- 消息列表项模块 -->\n    <MessageList :userLists=\"userLists\" :maibao=\"maibao\" :pushmsg=\"pushmsg\"></MessageList>\n  </Wrapper>\n</template>\n<script>\nimport messageApi from '../../../api/message/index'\nimport AppLoading from '../../../components/AppLoading/index'\nimport AppHeader from '../../../components/AppHeader'\nimport Wrapper from '../../../components/Wrapper/index'\nimport MessageList from '../../../components/MessageList/index'\nimport dialogService from '../../../service/dialogService'\nimport { mapState, mapActions } from 'vuex'\nexport default {\n  name: 'message-index',\n  data() {\n    return {\n      maibao: null, // 小脉宝消息\n      pushmsg: null, // 推送消息\n      userLists: [], // 用户消息列表\n      popRed: false, //小红点\n      firstIn: true // 第一次进入\n    }\n  },\n  computed: {\n    ...mapState({\n      userInfo: state => state.user.userInfo\n    })\n  },\n  components: {\n    AppLoading,\n    Wrapper,\n    AppHeader,\n    MessageList\n  },\n  async mounted() {\n    Promise.all([\n      messageApi.getXiaomaibao(),\n      messageApi.getUserlist(),\n      messageApi.getPush(1)\n    ])\n      .then(result => {\n        if (result[0].data.list.data) {\n          this.maibao = result[0].data.list.data[0]\n        }\n        this.popRed = result[0].data.pop_flag\n        this.userLists = result[1].data.list.data\n        if (result[2].data.list) {\n          this.pushmsg = result[2].data.list.data[0]\n        }\n        this.firstIn = false\n      })\n      .catch(error => {\n        console.log(error) // 失败了\n        this.firstIn = false\n      })\n  },\n  methods: {\n    ...mapActions(['system/updateBeVipPath']),\n    // 查看对我感兴趣列表\n    toFavour() {\n      if (this.userInfo.vip_level) {\n        this.$router.push('/message/favour')\n      } else {\n        dialogService.confirm('开通会员才能查看，是否去开通？', ok => {\n          if (ok) {\n            this['system/updateBeVipPath']('/message/favour')\n            this.$router.push('/mine/vipCenter')\n          }\n        })\n      }\n    }\n  }\n}\n</script>\n<style lang=\"scss\" scoped>\n.message-warpper {\n  color: #000;\n  width: 100%;\n  box-sizing: border-box;\n  min-height: 100vh;\n  background: #fff;\n  overflow: hidden;\n  position: relative;\n}\n.message-topbar {\n  padding: 0 78px;\n  height: 210px;\n  width: 100%;\n  box-sizing: border-box;\n  overflow: hidden;\n  display: flex;\n  border-bottom: 20px solid #f6f6f6;\n  justify-content: space-between;\n}\n.message-topbar-item {\n  font-size: 22px;\n  color: #666666;\n  padding-top: 30px;\n  /* width: 100%; */\n  text-align: center;\n\n  .icon-duiwoganxingqu {\n    background: linear-gradient(to right, #fb4646, #fa4545);\n  }\n  .icon-woderenqi {\n    background: linear-gradient(to right, #fb9635, #fb9535);\n  }\n  .icon-xitongxiaoxi {\n    background: linear-gradient(to right, #385df9, #365cf8);\n  }\n  .iconfont {\n    position: relative;\n    width: 90px;\n    height: 90px;\n    display: block;\n    margin: 0 auto 16px;\n    line-height: 90px;\n    color: #fff;\n    border-radius: 50%;\n    font-size: 40px;\n  }\n  .reddot {\n    position: absolute;\n    display: block;\n    width: 20px;\n    height: 20px;\n    border-radius: 50%;\n    border: 3px solid #fff;\n    background: #ef2323;\n    top: 0;\n    right: 0;\n  }\n}\n</style>"]}]}