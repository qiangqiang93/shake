  }\n    }\n  }\n  .btn {\n    width: 690px;\n    height: 88px;\n    background-color: #274ff3;\n    border-radius: 10px;\n    font-size: 34px;\n    color: #fff;\n    margin: 40px 30px 0;\n    &:disabled {\n      background: #d8d8d8;\n    }\n  }\n}\n",{"version":3,"sources":["step3.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsIA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"step3.vue","sourceRoot":"src/module/setting/payManage","sourcesContent":["<template>\n  <div class=\"revise-pay-pwd\">\n    <!-- 输入新密码 -->\n    <template v-if=\"step ===1\">\n      <div class=\"title\">请输入新的支付密码</div>\n      <input ref=\"pwd\" type=\"text\" readonly unselectable=\"on\" class=\"input\" autofocus maxlength=\"6\" v-model=\"value\" @input=\"inputHandler()\" />\n      <ul class=\"pwd-wrap\" @click=\"focus\">\n        <li>\n          <i v-if=\"value.length > 0\" class=\"iconfont icon-jiufuqianbaoicon14\"></i>\n        </li>\n        <li>\n          <i v-if=\"value.length > 1\" class=\"iconfont icon-jiufuqianbaoicon14\"></i>\n        </li>\n        <li>\n          <i v-if=\"value.length > 2\" class=\"iconfont icon-jiufuqianbaoicon14\"></i>\n        </li>\n        <li>\n          <i v-if=\"value.length > 3\" class=\"iconfont icon-jiufuqianbaoicon14\"></i>\n        </li>\n        <li>\n          <i v-if=\"value.length > 4\" class=\"iconfont icon-jiufuqianbaoicon14\"></i>\n        </li>\n        <li>\n          <i v-if=\"value.length > 5\" class=\"iconfont icon-jiufuqianbaoicon14\"></i>\n        </li>\n      </ul>\n    </template>\n    <!-- 确认密码 -->\n    <template v-else>\n      <div class=\"title\">请再次输入新的支付密码</div>\n      <input ref=\"pwd\" type=\"text\" class=\"input\" autofocus maxlength=\"6\" v-model=\"value\" @input=\"inputHandler()\" />\n      <ul class=\"pwd-wrap\" @click=\"focus\">\n        <li>\n          <i v-if=\"value.length > 0\" class=\"iconfont icon-jiufuqianbaoicon14\"></i>\n        </li>\n        <li>\n          <i v-if=\"value.length > 1\" class=\"iconfont icon-jiufuqianbaoicon14\"></i>\n        </li>\n        <li>\n          <i v-if=\"value.length > 2\" class=\"iconfont icon-jiufuqianbaoicon14\"></i>\n        </li>\n        <li>\n          <i v-if=\"value.length > 3\" class=\"iconfont icon-jiufuqianbaoicon14\"></i>\n        </li>\n        <li>\n          <i v-if=\"value.length > 4\" class=\"iconfont icon-jiufuqianbaoicon14\"></i>\n        </li>\n        <li>\n          <i v-if=\"value.length > 5\" class=\"iconfont icon-jiufuqianbaoicon14\"></i>\n        </li>\n      </ul>\n      <button class=\"btn\" :disabled=\"value.length===6 ?false :true\" @click=\"submit\">完成</button>\n    </template>\n  </div>\n</template>\n<script>\nimport httpService from '@/service/httpService'\nimport { mapActions, mapState } from 'vuex'\nimport dialogService from '@/service/dialogService'\nimport setting from '../../../api/setting'\nexport default {\n  name: 'payManageStep3',\n  data() {\n    return {\n      value: '',\n      newPwd: '', //新密码\n      step: 1 // 1第一次输入 ，2再次输入\n    }\n  },\n  computed: {\n    ...mapState({\n      user: state => state.user.userInfo,\n      toPath: state => state.system.beVipPath,\n      payTpye: state => state.system.payTpye\n    })\n  },\n  methods: {\n    ...mapActions(['user/setUserInfo', 'system/updateBeVipPath']),\n    focus() {\n      this.$refs.pwd.focus()\n    },\n    inputHandler() {\n      if (this.step !== 1) return false\n      if (this.value.length === 6) {\n        setTimeout(() => {\n          this.step = 2\n          this.newPwd = this.value\n          this.value = ''\n        }, 200)\n      }\n    },\n    async submit() {\n      let self = this\n      if (this.newPwd != this.value) {\n        dialogService.toast('2次密码输入不一致')\n      } else {\n        try {\n          let res = null\n          if (this.$route.query.sign) {\n            res = await setting.setPayPwd(this.value, this.$route.query.sign)\n          } else {\n            res = await setting.resetPayPwd(this.$route.query.pwd, this.value)\n          }\n          if (res.status !== 0) throw new Error(res.message)\n          dialogService.toast('设置成功', function() {\n            if (self.$route.query.sign && !self.user.payment_pwd) {\n              // 初始化支付密码时序改变状态\n              let obj = Object.assign({}, self.user)\n              obj.payment_pwd = true\n              self['user/setUserInfo'](obj)\n            }\n            if (self.payTpye) {\n              self.$router.replace(`/pay?type=${self.payTpye}`)\n              return\n            }\n            if (self.toPath) {\n              self.$router.replace(self.toPath)\n            } else {\n              self.$router.push('/setting/payManage')\n            }\n          })\n        } catch (err) {\n          dialogService.toast(err.message)\n        }\n      }\n    }\n  },\n  beforeRouteLeave(to, from, next) {\n    this['system/updateBeVipPath'](null)\n    next()\n  }\n}\n</script>\n<style scoped lang='scss'>\n.revise-pay-pwd {\n  width: 100%;\n  height: 100vh;\n  background: #f6f6f6;\n  .title {\n    padding: 44px 30px;\n    font-size: 26px;\n    color: #555555;\n    box-sizing: border-box;\n  }\n  .input {\n    left: 5%;\n    top: 0;\n    width: 90%;\n    opacity: 0;\n    position: absolute;\n    height: 1;\n    outline: none;\n  }\n  .pwd-wrap {\n    width: 90%;\n    display: flex;\n    margin: 0 auto;\n    padding-bottom: 1px;\n    justify-content: space-between;\n    li {\n      width: 100px;\n      height: 100px;\n      line-height: 100px;\n      text-align: center;\n      border: 1px solid #ddd;\n      background: #fff;\n      i {\n        font-size: 26px;\n      }\n    }\n  }\n  .btn {\n    width: 690px;\n    height: 88px;\n    background-color: #274ff3;\n    border-radius: 10px;\n    font-size: 34px;\n    color: #fff;\n    margin: 40px 30px 0;\n    &:disabled {\n      background: #d8d8d8;\n    }\n  }\n}\n</style>\n"]}]}