r(res.message)\n        dialogService.toast('短信验证码已发送,请注意查收')\n        this.countDown()\n      } catch (err) {\n        dialogService.toast(err.message)\n      }\n    },\n    async next() {\n      if (this.verifyCode.length === 0) {\n        dialogService.toast('验证码不能为空')\n      } else {\n        try {\n          let res = await setting.sign(this.user.phone, this.verifyCode)\n          if (res.status !== 0) throw new Error(res.message)\n          this.$router.push({\n            path: '/setting/payManage/step3',\n            query: {\n              sign: res.data.sign\n            }\n          })\n        } catch (err) {\n          dialogService.toast(err.message)\n        }\n      }\n    }\n  },\n  components: {\n    ListItem,\n    List,\n    AppHeader,\n    Wrapper\n  },\n  computed: {\n    ...mapState({\n      user: state => state.user.userInfo,\n      platform: state => state.system.platform\n    }),\n    paddingTop: function() {\n      return this.platform === 'wechat' ? '0px' : '90px'\n    }\n  }\n}\n",{"version":3,"sources":["step1.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;AAsBA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"step1.vue","sourceRoot":"src/module/setting/payManage","sourcesContent":["<template>\n  <Wrapper class=\"binging-cell-phone\" :paddingTop=\"paddingTop\">\n   <AppHeader title=\"验证手机\" ref=\"header\" ></AppHeader>\n    <div class=\"header\">\n      <img src=\"../phone.png\" alt=\"\">\n      <p class=\"phone\">当前绑定手机号:&nbsp;{{user.phone}}</p>\n    </div>\n    <List>\n      <ListItem label=\"验证码\">\n        <div class=\"margin-left\">\n          <input type=\"number\" maxlength=\"6\" v-model=\"verifyCode\" placeholder=\"输入验证码\">\n          <p @click=\"getCode\">{{tips}}</p>\n        </div>\n      </ListItem>\n    </List>\n    <p class=\"tip\">若不能获取验证码，请联系客服023-68452158</p>\n    <button class=\"btn\" :disabled=\"!verifyCode\" @click=\"next\">\n      下一步\n    </button>\n  </Wrapper>\n</template>\n<script>\nimport { mapState } from 'vuex'\nimport List from '../../../components/List'\nimport ListItem from '../../../components/ListItem'\nimport system from '../../../api/system'\nimport setting from '../../../api/setting'\nimport dialogService from '@/service/dialogService'\nimport AppHeader from '../../../components/AppHeader'\nimport Wrapper from '../../../components/Wrapper'\nexport default {\n  name: 'payManageStep1',\n  data() {\n    return {\n      timer: null,\n      active: true, // true 可发送验证码，false不可以\n      verifyCode: '', // 验证码\n      tips: '获取验证码' // 验证码提示\n    }\n  },\n  methods: {\n    // 验证码倒计时\n    countDown() {\n      let second = 60\n      this.active = false\n      this.tips = second + 's'\n      this.timer = setInterval(() => {\n        second--\n        this.tips = second + 's'\n        if (second === 0) {\n          this.active = true\n          this.tips = '获取验证码'\n          clearInterval(this.timer)\n        }\n      }, 1000)\n    },\n    async getCode() {\n      if (!this.active) return false\n      try {\n        let res = await system.sendValidateCode(this.user.phone, 5)\n        if (res.status !== 0) throw new Error(res.message)\n        dialogService.toast('短信验证码已发送,请注意查收')\n        this.countDown()\n      } catch (err) {\n        dialogService.toast(err.message)\n      }\n    },\n    async next() {\n      if (this.verifyCode.length === 0) {\n        dialogService.toast('验证码不能为空')\n      } else {\n        try {\n          let res = await setting.sign(this.user.phone, this.verifyCode)\n          if (res.status !== 0) throw new Error(res.message)\n          this.$router.push({\n            path: '/setting/payManage/step3',\n            query: {\n              sign: res.data.sign\n            }\n          })\n        } catch (err) {\n          dialogService.toast(err.message)\n        }\n      }\n    }\n  },\n  components: {\n    ListItem,\n    List,\n    AppHeader,\n    Wrapper\n  },\n  computed: {\n    ...mapState({\n      user: state => state.user.userInfo,\n      platform: state => state.system.platform\n    }),\n    paddingTop: function() {\n      return this.platform === 'wechat' ? '0px' : '90px'\n    }\n  }\n}\n</script>\n\n<style scoped lang='scss'>\n.binging-cell-phone {\n  height: 100vh;\n  background: #f6f6f6;\n  overflow: hidden;\n  .header {\n    width: 100%;\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: center;\n    img {\n      width: 160px;\n      margin-top: 40px;\n      height: 272px;\n      margin-left: 30px;\n    }\n    .phone {\n      width: 100%;\n      text-align: center;\n      font-size: 30px;\n      color: #000000;\n      margin-top: 40px;\n    }\n  }\n  .margin-left {\n    width: 80%;\n    outline: none;\n    display: flex;\n    justify-content: space-between;\n    p {\n      font-size: 24px;\n      color: #274ff3;\n      line-height: 36px;\n    }\n    input {\n      outline: none;\n    }\n  }\n  .btn {\n    width: 690px;\n    height: 88px;\n    background-color: #274ff3;\n    border-radius: 10px;\n    font-size: 34px;\n    color: #fff;\n    margin: 40px 30px 0;\n    &:disabled {\n      background: #d8d8d8;\n    }\n  }\n  .tip {\n    font-size: 24px;\n    line-height: 30px;\n    color: #999999;\n    padding-left: 30px;\n    margin-top: 20px;\n  }\n}\n</style>\n"]}]}