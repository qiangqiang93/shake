AA,OAAA;AAAA,KAFA;AAGA,IAAA,QAAA,EAAA,kBAAA,KAAA;AAAA,aAAA,KAAA,CAAA,MAAA,CAAA,QAAA;AAAA;AAHA,GAAA,CADA;AAlCA,CAAA","sourcesContent":["<!--\n    @name       AppDialog\n    @desc       通用弹窗组件\n    @props\n        dialog: {\n            type        类型[alert、confirm、tips]\n            message     信息\n            callback    回调函数\n        }\n-->\n<template>\n  <div>\n    <div class=\"layer-wraper\" v-show=\"type === 'alert' || type === 'confirm'\" @click=\"cancelClick()\">\n      <div class=\"layer-content\" v-show=\"type ==='alert' ||type === 'confirm'\" @click.stop>\n        <div class=\"message\" v-text=\"message\"></div>\n        <div class=\"footer\">\n          <p class=\"button\" @click.stop=\"cancelClick()\" v-show=\"type === 'confirm'\">取消</p>\n          <p class=\"button\" @click.stop=\"okClick()\">确定</p>\n        </div>\n      </div>\n    </div>\n    <div class=\"tips\" v-show=\"type =='tips'\" v-text=\"message\"></div>\n  </div>\n</template>\n\n<script>\nimport { mapState, mapActions } from 'vuex'\nexport default {\n  name: 'AppDialog',\n  data() {\n    return {\n      time: null // toast 自动隐藏定时器\n    }\n  },\n  methods: {\n    ...mapActions('dialog', ['clearDialog']),\n    okClick() {\n      this.callback && this.callback(true)\n      this.clearDialog()\n    },\n    cancelClick() {\n      this.callback && this.callback(false)\n      this.clearDialog()\n    }\n  },\n  watch: {\n    type() {\n      if (this.type == 'tips') {\n        if (this.time) {\n          clearTimeout(this.time)\n        }\n        this.time = setInterval(() => {\n          this.callback && this.callback()\n          this.clearDialog()\n        }, 1500)\n      } else {\n        clearTimeout(this.time)\n        this.time = null\n      }\n    }\n  },\n  computed: {\n    ...mapState({\n      type: state => state.dialog.type,\n      message: state => state.dialog.message,\n      callback: state => state.dialog.callback\n    })\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.layer-wraper {\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  z-index: 9999;\n  display: flex;\n  position: fixed;\n  align-items: center;\n  justify-content: center;\n  background: rgba($color: #000000, $alpha: 0.3);\n}\n\n@keyframes show {\n  from {\n    transform: scale(0);\n  }\n  to {\n    transform: scale(1);\n  }\n}\n@keyframes showtip {\n  from {\n    opacity: 0;\n  }\n  to {\n    opacity: 1;\n  }\n}\n.tips {\n  top: 50%;\n  left: 50%;\n  color: #fff;\n  padding: 30px;\n  font-size: 32px;\n  position: fixed;\n  border-radius: 6px;\n  animation: showtip 0.5s;\n  transform: translate(-50%, -50%);\n  background: rgba($color: #000000, $alpha: 0.8);\n}\n.layer-content {\n  width: 620px;\n  height: 320px;\n  display: flex;\n  padding-top: 50px;\n  background: #fff;\n  border-radius: 6px;\n  animation: show 0.3s;\n  box-sizing: border-box;\n  flex-direction: column;\n  font-family: 'PingFang SC';\n  .message {\n    flex: 1;\n    padding: 30px 40px 0;\n    font-size: 32px;\n    color: #666666;\n    text-align: center;\n  }\n  .footer {\n    width: 100%;\n    height: 110px;\n    display: flex;\n    line-height: 110px;\n    position: relative;\n    box-sizing: border-box;\n    border-top: 1px solid #eaeaea;\n    .button {\n      width: 100%;\n      font-size: 34px;\n      color: #666666;\n      font-weight: bold;\n      text-align: center;\n      &:last-child {\n        color: #374ff3;\n        border-left: 1px solid #eaeaea;\n      }\n    }\n  }\n}\n</style>"],"sourceRoot":"src/components/AppDialog"}]}