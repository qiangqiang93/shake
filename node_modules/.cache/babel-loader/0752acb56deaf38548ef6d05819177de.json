v>\n\n</template>\n<script>\n  import Swiper from 'swiper'\n  import 'swiper/dist/css/swiper.min.css'\n  export default {\n    data() {\n      return {\n        slides: [require('./1.jpg'), require('./2.jpg'), require('./3.jpg')],\n        bgSrc: '',\n        isLoading: true // 是否正在加载\n      }\n    },\n    mounted() {\n      var newImg = new Image()\n      newImg.src = '/static/merchantsBg.png'\n      newImg.onload = () => {\n        this.isLoading = false\n\n        this.bgSrc = newImg.src\n        this.numberGrow(this.$refs.num1, {\n          time: 2,\n          value: 31\n        })\n        this.numberGrow(this.$refs.num2, {\n          time: 2.5,\n          value: 2170\n        })\n        this.numberGrow(this.$refs.num3, {\n          time: 3,\n          value: 21700\n        })\n        new Swiper('.swiper-container', {\n          autoplay: {\n            delay: 3000,\n            disableOnInteraction: false\n          },\n          loop: true,\n          pagination: {\n            el: '.swiper-pagination'\n          },\n          observer: true, // 修改swiper自己或子元素时，自动初始化swiper\n          observeParents: true // 修改swiper的父元素时，自动初始化swiper\n        })\n      }\n\n    },\n    methods: {\n      numberGrow(ele, option) {\n        let self = this\n        option.time = option.time ? option.time : 1 //默认为1s\n        let step = (option.value * 10) / (option.time * 1000) //步长\n        let start = 0 //作为一个累加值\n        let current = 0 //要显示再文本处的\n        let t = setInterval(function () {\n          start += step\n          if (start > option.value) {\n            clearInterval(t)\n            start = option.value\n            t = null\n          }\n          if (current === start) {\n            //当两者相等就不需要执行下面的代码\n            return\n          }\n          current = start\n          ele.innerHTML = parseInt(current)\n        }, 10)\n      }\n    }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n  .merchants {\n    position: relative;\n    background: #fff;\n    overflow: hidden;\n    margin-bottom: 60px;\n\n    img {\n      width: 100%;\n    }\n\n    .num {\n      position: absolute;\n      top: 440px;\n      font-size: 40px;\n      font-weight: 400;\n      width: 140px;\n      text-align: right;\n      padding-right: 10px;\n      box-sizing: border-box;\n    }\n\n    .num1 {\n      left: 0;\n    }\n\n    .num2 {\n      left: 292px;\n    }\n\n    .num3 {\n      right: 65px;\n    }\n\n    .swiper-container {\n      width: 670px;\n      height: 400px;\n\n      img {\n        object-fit: cover;\n        width: 100%;\n        height: 365px;\n      }\n    }\n\n    /deep/ .swiper-pagination-bullet {\n      width: 40px;\n      height: 5px;\n      border-radius: 0;\n    }\n\n    .info {\n      width: 615px;\n      margin-left: 40px;\n      color: #333;\n      font-size: 26px;\n      line-height: 36px;\n\n      >div {\n        display: flex;\n        margin-top: 40px;\n\n        span {\n          &:nth-of-type(1) {\n            width: 180px;\n          }\n\n          &:nth-of-type(2) {\n            flex: 1;\n          }\n\n          a {\n            color: #333;\n            text-decoration: none;\n            display: inline-block;\n            width: 100%;\n            height: 100%;\n          }\n        }\n      }\n\n      i {\n        font-size: 26px;\n        margin-right: 20px;\n      }\n    }\n  }\n</style>"],"sourceRoot":"src/module/activity/merchants"}]}