item.type==3\">\n            <p class=\"button\">同意</p>\n            <p class=\"button refuse\">拒绝</p>\n          </div>\n          <em></em>\n          <span></span>\n        </div>\n      </div>\n    </div>\n  </Wrapper>\n</template>\n<script>\nimport dialogService from '../../../service/dialogService'\nimport messageApi from '../../../api/message/index'\nimport Wrapper from '../../../components/Wrapper/index'\nimport AppLoading from '../../../components/AppLoading/index'\nexport default {\n  name: 'message-user',\n  data() {\n    return {\n      loading: false, // 是否在加载\n      windowHeight: null, // 可见区域高度\n      last_page: null, // 总页数\n      page: 1, // 当前页数\n      firstIn: true,\n      pic: window.msgPortrait,\n      lists: []\n    }\n  },\n  components: { Wrapper, AppLoading },\n  async mounted() {\n    this.getlist()\n  },\n  methods: {\n    // 获取列表\n    async getlist(load) {\n      let that = this\n      this.loading = true\n      if (load) this.page += 1\n      let res = await messageApi.getUserdetail(this.$route.query.id)\n      if (load) {\n        res.data.list.data.forEach(element => {\n          that.lists.unshift(element)\n        })\n        this.$nextTick(() => {\n          that.windowHeight = document.documentElement.scrollHeight\n          window.scrollTo(0, that.windowHeight - that.last_height)\n          that.last_height = that.windowHeight\n        })\n      } else {\n        this.lists = res.data.list.data.reverse()\n        this.$nextTick(() => {\n          window.scrollTo(0, document.documentElement.scrollHeight)\n          this.last_height = document.documentElement.scrollHeight\n          this.firstIn = false\n        })\n      }\n      this.last_page = res.data.list.last_page\n      this.windowHeight = document.documentElement.clientHeight\n      this.loading = false\n    },\n    nextPage(scrolled, scrollHeight) {\n      let scrollTop = scrolled < 1\n      if (!scrollTop || this.loading || this.page >= this.last_page) {\n        return false\n      } else {\n        this.getlist(true)\n      }\n    }\n  }\n}\n</script>\n<style lang=\"scss\" scoped>\n.msg-detail-tips {\n  font-size: 28px;\n  text-align: center;\n  margin-top: 40px;\n  padding-bottom: 20px;\n}\n.warpper {\n  .tips {\n    font-size: 28px;\n    text-align: center;\n    margin-top: 40px;\n    padding-bottom: 20px;\n  }\n  .message {\n    .box {\n      display: flex;\n      .avatar {\n        width: 80px;\n        height: 80px;\n        border-radius: 50%;\n        margin-right: 30px;\n      }\n      .ctbox {\n        .toggle {\n          .button {\n            width: 145px;\n            height: 50px;\n            box-sizing: border-box;\n            font-size: 26px;\n            line-height: 50px;\n            text-align: center;\n            color: #fff;\n            background: #274ff3;\n            border-radius: 6px;\n          }\n          .refuse {\n            background: #ffffff;\n            color: #274ff3;\n            border: 1px solid #274ff3;\n          }\n          margin-top: 50px;\n          width: 100%;\n          display: flex;\n          justify-content: space-around;\n        }\n        .go {\n          font-size: 26px;\n          color: #274ff3;\n          text-align: right;\n          margin-top: 20px;\n        }\n        .content {\n          color: #333333;\n          font-size: 28px;\n          line-height: 45px;\n        }\n        em {\n          display: block;\n          border-width: 14px;\n          position: absolute;\n          top: 30px;\n          left: -30px;\n          border-style: dashed solid dashed;\n          border-color: transparent #e9e9e9 transparent transparent;\n          font-size: 0;\n          line-height: 0;\n        }\n        span {\n          display: block;\n          border-width: 14px;\n          position: absolute;\n          top: 30px;\n          left: -27px;\n          border-style: dashed solid dashed;\n          border-color: transparent #fff transparent transparent;\n          font-size: 0;\n          line-height: 0;\n        }\n        position: relative;\n        flex: 1;\n        background: #fff;\n        padding: 20px;\n        border-radius: 10px;\n        border: 1px solid #eaeaea;\n      }\n    }\n    .time {\n      color: #666666;\n      font-size: 22px;\n      margin-bottom: 30px;\n      text-align: center;\n    }\n    width: 100%;\n    margin-top: 35px;\n    padding: 0 30px;\n    box-sizing: border-box;\n  }\n  color: #000;\n  width: 100%;\n  box-sizing: border-box;\n  min-height: 100vh;\n  background: #f4f4f4;\n  overflow: hidden;\n  position: relative;\n}\n</style>\n \n "],"sourceRoot":"src/module/message/user"}]}