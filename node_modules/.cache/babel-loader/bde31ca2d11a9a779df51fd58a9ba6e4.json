)\">\n    <div class=\"slider-wrapper\">\n      <SlideShow :isEdit=\"false\" :input=\"graphics\" :index=\"index\"></SlideShow>\n    </div>\n    <div class=\"spread-mask\">\n      <div class=\"spread-info\">\n        <span class=\"spread-date\">{{spreadAD.created_at|timeFormt('YYYY/MM/DD')}}</span>\n        <span class=\"spread-coin\">分享可得{{spreadAD.maibao_spend_num || 0}}个脉宝</span>\n      </div>\n      <div class=\"spread-txt\" @click=\"goMarketing()\">\n        <span v-text=\"graphics[index] ? graphics[index].txt : ''\"></span>\n        <span v-if=\"spreadAD.user_marketing_id\" class=\"iconfont icon-chaolianjie\"></span>\n      </div>\n    </div>\n    <div class=\"ad-info\">\n      <div class=\"spread-card\" v-if=\"spreadAD.card_id\" @click=\"goToCard()\">\n        <img :src=\"spreadAD.head_picture\" alt=\"\">\n      </div>\n      <div class=\"spread-visited\">\n        <span class=\"iconfont icon-liulan\"></span>\n        <span class=\"count\" v-text=\"spreadAD.browse_num\"></span>\n      </div>\n      <div class=\"spread-shared\">\n        <span class=\"iconfont icon-share1\"></span>\n        <span class=\"count\" v-text=\"spreadAD.share_num\"></span>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport spread from '../../../api/spread'\nimport { mapActions, mapState } from 'vuex'\n// import sdkService from '@/service/sdkService'\nimport SlideShow from '../../../components/SlideShow'\nimport dialogService from '../../../service/dialogService'\nexport default {\n  name: 'spreadDetail',\n  data() {\n    return {\n      index: 0,\n      spreadAD: {},\n      graphics: [] // 图文内容副本\n    }\n  },\n  beforeMount() {\n    this.init()\n  },\n  methods: {\n    async init() {\n      try {\n        let res = await spread.getSingleAD(this.$route.params.id)\n        if (res.status !== 0) throw new Error(res.message)\n        this.spreadAD = res.data\n        // this.sdkConfig()\n        try {\n          this.graphics = JSON.parse(this.spreadAD.content)\n        } catch (err) {\n          this.graphics = []\n        }\n      } catch (err) {\n        dialogService.alert(err.message)\n      }\n    },\n    goToCard() {\n      this.$router.push({\n        path: `/card/detail/${this.spreadAD.user_id}`\n      })\n    },\n    goMarketing() {\n      if (this.spreadAD.user_marketing_id) {\n        this.$router.push({\n          path: `/user/tools/marketing/detail/${\n            this.spreadAD.user_marketing_id\n          }`\n        })\n      }\n    },\n    touchHandler(event, type) {\n      if (type === 'start') {\n        this.pos = event.touches[0].pageX\n      } else {\n        let pos = event.changedTouches[0].pageX\n        if (pos - this.pos >= 40) {\n          // 左滑\n          this.index =\n            this.index !== 0 ? (this.index = this.index - 1) : this.index\n        } else if (pos - this.pos <= -40) {\n          // 右滑\n          this.index =\n            this.index !== this.graphics.length - 1\n              ? (this.index = this.index + 1)\n              : this.index\n        }\n        this.pos = null\n      }\n    }\n    // sdkConfig() {\n    //   // 先判断是否审核通过\n    //   if (this.spreadAD.status === 2 || this.spreadAD.status === 3) {\n    //     let url = `/spread/detail/${this.spreadAD.id}`\n    //     sdkService.share({\n    //       title: this.spreadAD.title,\n    //       imgUrl: this.spreadAD.cover,\n    //       desc: this.spreadAD.motto || '汇脉宝——共享流量，专注营销',\n    //       link: `${config.host}/?#/share?target=${encodeURIComponent(url)}`\n    //     })\n    //   } else {\n    //     let url = `/`\n    //     sdkService.share({\n    //       title: '汇脉宝营销',\n    //       desc: '汇脉宝——共享流量，专注营销',\n    //       imgUrl: `${config.host}/static/full-logo.png`,\n    //       link: config.host + '/?#/article'\n    //     })\n    //   }\n    // }\n  },\n  components: {\n    SlideShow\n  },\n  computed: {\n    ...mapState({\n      user: state => state.user.userInfo\n    })\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.spread-wrapper {\n  width: 100%;\n  height: 100vh;\n  overflow: hidden;\n  background: #1b1b1b;\n\n  .slider-wrapper {\n    width: 100%;\n    height: 100%;\n  }\n  .spread-info {\n    height: 50px;\n    font-size: 26px;\n    color: #999999;\n    margin-top: 30px;\n    line-height: 50px;\n    font-family: 'PingFang SC Medium';\n    .spread-date {\n      float: left;\n      margin-left: 20px;\n    }\n    .spread-coin {\n      float: right;\n      margin-right: 20px;\n    }\n  }\n  .spread-txt {\n    color: #fff;\n    font-size: 32px;\n    margin-top: 40px;\n    text-indent: 20px;\n    font-family: 'PingFang SC Medium';\n  }\n  .icon-chaolianjie {\n    color: #46a9ff;\n  }\n  .ad-info {\n    top: 320px;\n    right: 20px;\n    width: 80px;\n    color: #fff;\n    font-size: 24px;\n    position: absolute;\n    text-align: center;\n    font-family: 'PingFang SC Medium';\n    .spread-card {\n      width: 80px;\n      height: 80px;\n      overflow: hidden;\n      border-radius: 50%;\n    }\n    img {\n      width: 100%;\n      height: 100%;\n    }\n    .spread-visited,\n    .spread-shared {\n      display: flex;\n      margin-top: 50px;\n      align-items: center;\n      flex-direction: column;\n      justify-content: center;\n      text-shadow: 0px 2px 6px rgba(0, 0, 0, 0.2);\n    }\n    .icon-liulan {\n      font-size: 28px;\n    }\n    .icon-share1 {\n      font-size: 36px;\n    }\n    .count {\n      margin-top: 10px;\n    }\n  }\n  .spread-mask {\n    left: 0;\n    bottom: 0;\n    width: 100%;\n    position: absolute;\n    padding-bottom: 60px;\n    box-sizing: border-box;\n    background: linear-gradient(0deg, rgba(27, 27, 27, 0.3), rgba(0, 0, 0, 0));\n  }\n}\n</style>\n"],"sourceRoot":"src/module/spread/detail"}]}