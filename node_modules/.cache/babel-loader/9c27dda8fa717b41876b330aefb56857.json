t default {\n  props: ['item', 'operate'],\n  data() {\n    return {}\n  },\n  mounted() {},\n  methods: {\n    // 跳转到营销网页详情\n    goMarketingDetail() {\n      this.$router.push(`/marketing/detail/${this.item.id}`)\n    },\n    // 显示分享引导\n    showShareGuide() {\n      shareService()\n    },\n    // 管理\n    operateItem() {\n      this.$emit('operate')\n    }\n  },\n  computed: {\n    posterStyle: function() {\n      return {\n        'background-image': `url(${getMarketingCover(this.item.contents)})`\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.marketing-item {\n  width: 100%;\n  height: 160px;\n  display: flex;\n  overflow: hidden;\n  position: relative;\n  background: #fff;\n  box-sizing: border-box;\n}\n.marketing-item-poster {\n  width: 290px;\n  height: 160px;\n  overflow: hidden;\n  margin-right: 20px;\n  border-radius: 6px;\n  box-sizing: border-box;\n  background-size: cover;\n  background-position: center;\n}\n.marketing-item-info {\n  flex: 1;\n  /* width: 320px; */\n  height: 160px;\n  overflow: hidden;\n}\n.marketing-item-title {\n  height: 28px;\n  font-size: 28px;\n  color: #000000;\n  overflow: hidden;\n  line-height: 28px;\n  white-space: nowrap;\n  text-overflow: ellipsis;\n  font-family: 'PingFang SC Medium';\n}\n.marketing-item-date,\n.marketing-item-counter {\n  font-size: 20px;\n  color: #999999;\n  overflow: hidden;\n  font-family: 'PingFang SC Medium';\n}\n.marketing-item-shared,\n.marketing-item-visited {\n  float: left;\n  width: 114px;\n  height: 26px;\n  display: block;\n  margin-top: 15px;\n  line-height: 26px;\n  .iconfont {\n    float: left;\n    display: block;\n    font-size: 26px;\n    margin-right: 18px;\n  }\n  em {\n    float: left;\n    display: block;\n    font-style: normal;\n  }\n}\n.marketing-item-date {\n  margin-top: 16px;\n}\n.marketing-item-operate {\n  font-size: 24px;\n  color: #333333;\n  padding-top: 28px;\n  text-align: right;\n  line-height: 24px;\n  font-family: 'PingFang SC Medium';\n  .marketing-item-share {\n    margin-right: 36px;\n  }\n  .icon-fenxiang {\n    font-size: 22px;\n  }\n  .icon-setting {\n    font-size: 26px;\n  }\n}\n.cant-operate {\n  height: 124px;\n  .marketing-item-poster {\n    width: 220px;\n    height: 124px;\n    margin-right: 30px;\n    border-radius: 8px;\n  }\n  .marketing-item-info {\n    height: 124px;\n    display: flex;\n    flex-direction: column;\n    justify-content: space-around;\n  }\n  .marketing-item-title {\n    font-size: 30px;\n  }\n  .marketing-item-shared,\n  .marketing-item-visited {\n    margin: 0;\n  }\n  .marketing-item-date,\n  .marketing-item-counter {\n    margin: 0;\n    font-size: 22px;\n  }\n}\n</style>\n"],"sourceRoot":"src/components/MarketingItem"}]}