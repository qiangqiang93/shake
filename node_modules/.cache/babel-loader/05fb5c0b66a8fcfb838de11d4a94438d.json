, // 可见区域高度\n      last_page: null, // 总页数\n      page: 1, // 当前页数\n      fistIn: true, // 是否第一次进入\n      lists: [] //列表\n    }\n  },\n  components: { Wrapper, AppLoading },\n  mounted() {\n    this.getlist()\n  },\n  methods: {\n    async getlist(load) {\n      let that = this\n      this.loading = true\n      if (load) this.page += 1\n      let res = await messageApi.getXiaomaibao(this.page)\n      if (load) {\n        res.data.list.data.forEach(element => {\n          that.lists.unshift(element)\n        })\n        this.$nextTick(() => {\n          that.windowHeight = document.documentElement.scrollHeight\n          window.scrollTo(0, that.windowHeight - that.last_height)\n          that.last_height = that.windowHeight\n        })\n      } else {\n        this.lists = res.data.list.data.reverse()\n        this.$nextTick(() => {\n          window.scrollTo(0, document.documentElement.scrollHeight)\n          this.last_height = document.documentElement.scrollHeight\n          this.firstIn = false\n        })\n        this.fistIn = false\n      }\n      this.last_page = res.data.list.last_page\n      this.windowHeight = document.documentElement.clientHeight\n      this.loading = false\n    },\n    nextPage(scrolled, scrollHeight) {\n      let scrollTop = scrolled < 1\n      if (!scrollTop || this.loading || this.page >= this.last_page) {\n        return false\n      } else {\n        this.getlist(true)\n      }\n    }\n  }\n}\n</script>\n<style lang=\"scss\" scoped>\n.warpper {\n  .tips {\n    font-size: 28px;\n    text-align: center;\n    margin-top: 40px;\n    padding-bottom: 20px;\n  }\n  .nolist {\n    text-align: center;\n    padding-top: 50px;\n    font-size: 28px;\n  }\n  .message {\n    .box {\n      .type {\n        text-align: right;\n        color: #666666;\n        font-size: 24px;\n      }\n      .revenue {\n        color: #000000;\n        font-size: 28px;\n        overflow: hidden;\n        margin-bottom: 20px;\n        .content {\n          float: left;\n          width: 80%;\n          overflow: hidden;\n          text-overflow: ellipsis;\n          white-space: nowrap;\n          text-align: left;\n        }\n        .money {\n          float: right;\n        }\n      }\n      .err {\n        color: #f81e1e !important;\n      }\n      .status {\n        color: #49be03;\n        text-align: left;\n      }\n      .num {\n        font-size: 48px;\n        color: #333333;\n        text-align: center;\n        margin: 38px 0 20px;\n      }\n      .tips {\n      }\n      font-size: 26px;\n      border: 1px solid #eaeaea;\n      width: 100%;\n      border-radius: 10px;\n      overflow: hidden;\n      background: #ffffff;\n      padding: 30px 30px 40px;\n      box-sizing: border-box;\n    }\n    .time {\n      color: #666666;\n      font-size: 22px;\n      margin-bottom: 30px;\n    }\n    width: 100%;\n    margin-top: 35px;\n    padding: 0 30px;\n    box-sizing: border-box;\n    text-align: center;\n  }\n  color: #000;\n  width: 100%;\n  box-sizing: border-box;\n  min-height: 100vh;\n  background: #f4f4f4;\n  overflow: hidden;\n  position: relative;\n}\n</style>\n \n "],"sourceRoot":"src/module/message/xiaomaibao"}]}