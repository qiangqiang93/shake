;;AACA,eAAA,CAAA;AACA,iBAAA,OAAA,CAAA,IAAA,wBACA,KAAA,OAAA,CAAA,SADA,6BAEA,KAAA,MAFA;AAKA;;AACA,kBA7BA,CA8BA;;AA9BA;AAgCA,OAlCA,MAkCA;AACA,QAAA,YAAA;AACA;AACA;AAxCA,GANA;AAgDA,EAAA,QAAA;AACA,IAAA,OAAA,EAAA,mBAAA;AACA,aAAA,KAAA,IAAA,CAAA,2BAAA;AACA,KAHA;AAIA,IAAA,OAAA,EAAA,mBAAA;AACA,aAAA,KAAA,IAAA,CAAA,mCAAA;AACA;AANA,KAOA,QAAA,CAAA;AACA,IAAA,MAAA,EAAA,gBAAA,KAAA;AAAA,aAAA,KAAA,CAAA,IAAA,CAAA,QAAA,CAAA,EAAA;AAAA,KADA;AAEA,IAAA,QAAA,EAAA,kBAAA,KAAA;AAAA,aAAA,KAAA,CAAA,MAAA,CAAA,QAAA;AAAA;AAFA,GAAA,CAPA,CAhDA;AA4DA,EAAA,UAAA,EAAA;AACA,IAAA,IAAA,EAAA;AADA;AA5DA,CAAA","sourcesContent":["<!--\n    @name       CircleItem\n    @desc       互推圈分享记录、分享任务\n    @props      \n        item    <Object>    当前项\n        type    <Enum>      \n                    [\n                        'shared'    互相分享\n                        'unShare'   对方为分享\n                        'task'      分享任务\n                    ]\n-->\n\n<template>\n  <div class=\"circle-item\">\n    <div class=\"circle-item-item bas-border\" v-if=\"type !== 'task'\">\n      <div class=\"circle-item-user\">\n        <div class=\"circle-item-header\">\n          <img v-if=\"shareMe.head_portrait\" :src=\"shareMe.head_portrait\" alt=\"\" class=\"circle-item-head-img\">\n          <img v-else src=\"/static/img/default-header.png\" alt=\"\" class=\"circle-item-head-img\">\n        </div>\n        <p class=\"circle-item-name\" v-text=\"shareMe.user_name || '未命名'\"></p>\n        <p class=\"item-browse-num\" v-if=\"type === 'shared'\">\n          为我带来<span class=\"browse-num\" v-text=\"shareMe.browse_num\"></span>次浏览\n        </p>\n        <p class=\"item-browse-remind\" v-if=\"type === 'unShare'\" :class=\"{'disabled': shareMe.is_remind}\">\n          <span v-text=\"shareMe.is_remind ? '已提醒分享' : '提醒他分享'\"></span>\n        </p>\n      </div>\n      <div class=\"circle-item-content\">\n        <template v-if=\"shareMe.source_type === 'card'\">\n          <div class=\"circle-item-poster\">\n            <Crad class=\"circle-card\"></Crad>\n          </div>\n          <div class=\"circle-item-infos\">\n            <p class=\"item-infos-name\" v-text=\"shareMe.title\"></p>\n            <p class=\"item-infos-info\">\n              <span class=\"item-info-date\" v-text=\"'产品经理'\"></span>\n              <span class=\"item-info-cutline\">|</span>\n              <span class=\"item-info-count\" v-text=\"'互联网'\"></span>\n            </p>\n            <p class=\"item-infos-info\">\n              <span class=\"item-info-date\">{{item.created_at}}</span>\n              <span class=\"item-info-count\" v-text=\"'浏览' + shareMe.browse_num\"></span>\n            </p>\n          </div>\n        </template>\n        <template v-else>\n          <div class=\"circle-item-poster\" :style=\"{'background-image': 'url('+ shareMe.cover+')'}\"></div>\n          <div class=\"circle-item-infos\">\n            <p class=\"item-infos-name\" v-text=\"shareMe.title\"></p>\n            <p class=\"item-infos-info\">\n              <span class=\"item-info-date\">{{item.created_at}}</span>\n              <span class=\"item-info-count\" v-text=\"'浏览' + shareMe.browse_num\"></span>\n            </p>\n          </div>\n        </template>\n      </div>\n    </div>\n    <div class=\"circle-item-item\">\n      <div class=\"circle-item-user\">\n        <div class=\"circle-item-header\">\n          <img v-if=\"myShare.head_portrait\" :src=\"myShare.head_portrait\" alt=\"\" class=\"circle-item-head-img\">\n          <img v-else src=\"/static/img/default-header.png\" alt=\"\" class=\"circle-item-head-img\">\n        </div>\n        <p class=\"circle-item-name\" v-text=\"myShare.user_name || '未命名'\"></p>\n        <p class=\"item-browse-num\" v-if=\"type === 'task'\" @click.stop=\"shareTask()\">\n          <i class=\"iconfont icon-fenxiang\"></i>\n        </p>\n        <p class=\"item-browse-num\" v-else>\n          我为他带来<span class=\"browse-num\" v-text=\"myShare.browse_num\"></span>次浏览\n        </p>\n      </div>\n      <div class=\"circle-item-content\">\n        <template v-if=\"myShare.source_type === 'card'\">\n          <div class=\"circle-item-poster\">\n            <Crad class=\"circle-card\"></Crad>\n          </div>\n          <div class=\"circle-item-infos\">\n            <p class=\"item-infos-name\" v-text=\"shareMe.title\"></p>\n            <p class=\"item-infos-info\">\n              <span class=\"item-info-date\" v-text=\"'产品经理'\"></span>\n              <span class=\"item-info-cutline\">|</span>\n              <span class=\"item-info-count\" v-text=\"'互联网'\"></span>\n            </p>\n            <p class=\"item-infos-info\">\n              <span class=\"item-info-date\">{{item.created_at}}</span>\n              <span class=\"item-info-count\" v-text=\"'浏览' + shareMe.browse_num\"></span>\n            </p>\n          </div>\n        </template>\n        <template v-else>\n          <div class=\"circle-item-poster\" :style=\"{'background-image': 'url('+ myShare.cover+')'}\"></div>\n          <div class=\"circle-item-infos\">\n            <p class=\"item-infos-name\" v-text=\"myShare.title\"></p>\n            <p class=\"item-infos-info\">\n              <span class=\"item-info-date\">{{item.created_at}}</span>\n              <span class=\"item-info-count\" v-text=\"'浏览' + myShare.browse_num\"></span>\n            </p>\n          </div>\n        </template>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Crad from '../Card'\nimport { mapState } from 'vuex'\nimport shareService from '../../service/shareService.js'\nexport default {\n  props: ['item', 'type'],\n  data() {\n    return {}\n  },\n  mounted() {},\n  methods: {\n    // 点击分享\n    shareTask() {\n      if (this.platform === 'wechat') {\n        // 根据不同类型跳转到不同的页面\n        switch (this.myShare.source_style) {\n          case 1:\n            this.$router.push(\n              `/article/detail/${this.myShare.source_id}/${\n                this.myShare.user_id\n              }?task=1&shareId=${this.userId}`\n            )\n            break\n          case 2:\n            this.$router.push(\n              `/webpage/detail/${this.myShare.source_id}?task=1&shareId=${\n                this.userId\n              }`\n            )\n            break\n          case 3:\n            this.$router.push(\n              `/marketing/detail/${this.myShare.source_id}?task=1&shareId=${\n                this.userId\n              }`\n            )\n            break\n          case 4:\n            this.$router.push(\n              `/card/detail/${this.myShare.source_id}?task=1&shareId=${\n                this.userId\n              }`\n            )\n            break\n          default:\n          // nothing\n        }\n      } else {\n        shareService()\n      }\n    }\n  },\n  computed: {\n    myShare: function() {\n      return this.item.inter_push_record_materials\n    },\n    shareMe: function() {\n      return this.item.passive_inter_push_record_materials\n    },\n    ...mapState({\n      userId: state => state.user.userInfo.id,\n      platform: state => state.system.platform\n    })\n  },\n  components: {\n    Crad\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.circle-item {\n  width: 690px;\n  /* height: 490px; */\n  padding: 0 25px;\n  background: #fff;\n  border-radius: 10px;\n  box-sizing: border-box;\n  font-family: 'PingFang SC Medium';\n  box-shadow: 0px 0px 10px 0px rgba(0, 0, 0, 0.05);\n}\n.circle-item-item {\n  width: 100%;\n  padding: 30px 0;\n  overflow: hidden;\n  position: relative;\n  box-sizing: border-box;\n  &.bas-border {\n    border-bottom: 1px solid #eaeaea;\n  }\n}\n.circle-item-user {\n  width: 100%;\n  height: 36px;\n  line-height: 36px;\n  position: relative;\n  margin-bottom: 25px;\n}\n.circle-item-header {\n  float: left;\n  width: 36px;\n  height: 36px;\n  overflow: hidden;\n  margin-right: 18px;\n  border-radius: 18px;\n}\n.circle-item-head-img {\n  width: 100%;\n  height: 100%;\n}\n.circle-item-name {\n  float: left;\n  font-size: 26px;\n  color: #000033;\n}\n.item-browse-num {\n  float: right;\n  font-size: 24px;\n  color: #666666;\n  .browse-num {\n    color: #34bd0a;\n  }\n  .icon-fenxiang {\n    font-size: 32px;\n  }\n}\n.item-browse-remind {\n  top: 0;\n  right: 0;\n  width: 180px;\n  height: 50px;\n  color: #fff;\n  font-size: 22px;\n  overflow: hidden;\n  line-height: 50px;\n  position: absolute;\n  text-align: center;\n  border-radius: 25px;\n  background: #2850f4;\n  &.disabled {\n    background: #fff;\n  }\n}\n.circle-item-content {\n  width: 100%;\n  height: 124px;\n  display: flex;\n  overflow: hidden;\n}\n.circle-item-poster {\n  width: 220px;\n  height: 124px;\n  margin-right: 25px;\n  border-radius: 8px;\n  background-size: cover;\n  background-position: center;\n}\n.circle-item-infos {\n  flex: 1;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-evenly;\n}\n.item-infos-name {\n  font-size: 30px;\n  color: #0a0a0a;\n  overflow: hidden;\n  display: -webkit-box;\n  -webkit-line-clamp: 2;\n  -webkit-box-orient: vertical;\n}\n.item-infos-info {\n  font-size: 22px;\n  color: #666666;\n}\n.item-info-date {\n  margin-right: 35px;\n}\n</style>\n"],"sourceRoot":"src/components/CircleItem"}]}