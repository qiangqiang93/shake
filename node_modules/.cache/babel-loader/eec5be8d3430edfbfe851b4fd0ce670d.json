dItem>\n        </div>\n      </div>\n      <div class=\"card-collection-list\" v-if=\"!cards.length\">\n        <img class=\"empty-img\" src=\"./empty.png\" alt=\"\">\n      </div>\n    </template>\n    <div class=\"card-collection-footer\">\n      <button class=\"button\" @click=\"selectAll()\" v-text=\"isSelectAll?'取消全选':'全选'\"></button>\n      <button class=\"button del\" @click=\"delCards()\">删除</button>\n    </div>\n  </Wrapper>\n</template>\n\n<script>\nimport card from '../../../api/card'\nimport { mapState, mapActions } from 'vuex'\nimport Loading from '../../../components/Loading'\nimport Wrapper from '../../../components/Wrapper'\nimport CardItem from '../../../components/CardItem'\nimport dialogService from '../../../service/dialogService'\nimport AndroidHeader from '../../../components/AndroidHeader'\nexport default {\n  data() {\n    return {\n      cards: [], // 名片夹名片列表\n      isEdit: false, // 是否编辑\n      keyWord: null, // 搜索关键字\n      isRequest: false, // 是否正在请求\n      isSelectAll: false, // 是否全选\n      windowHeight: null, // 窗口高度\n      gloableLoading: true // 全局加载\n    }\n  },\n  mounted() {\n    this.getCollectCard()\n    this.windowHeight = document.documentElement.clientHeight\n  },\n  methods: {\n    // 获取收藏的名片\n    async getCollectCard() {\n      try {\n        this.isRequest = true\n        let res = await card.getCollectCard()\n        if (res.status !== 0) throw new Error(res.message)\n        this.gloableLoading = false\n        this.isRequest = false\n        this.cards = res.data\n      } catch (err) {\n        dialogService.alert(err.message)\n      }\n    },\n    // 滚动处理函数\n    scrollHandler(scrolled, scrollHeight) {\n      //   if (this.isRequest) return false\n      //   let scrollBottom = this.windowHeight + scrolled > scrollHeight - 20\n      //   let shouldLoad = scrollBottom && this.searchParams.offset < this.count\n      //   if (shouldLoad) this.getList()\n    },\n    // 全选\n    selectAll() {\n      this.isSelectAll = !this.isSelectAll\n      this.cards = this.cards.map(item => {\n        item.select = this.select\n        return item\n      })\n    },\n    // 删除所选名片\n    delCards() {\n      dialogService.confirm('您确认删除已选名片', function(del) {\n        if (del) {\n          // 删除名片夹名片\n        }\n      })\n    }\n  },\n  computed: {\n    ...mapState({\n      platform: state => state.system.platform\n    }),\n    paddingTop: function() {\n      return this.platform !== 'wechat' && !this.isEdit ? '198px' : '80px'\n    }\n  },\n  components: {\n    Wrapper,\n    Loading,\n    CardItem,\n    AndroidHeader\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.card-collection-header {\n  top: 0;\n  width: 100%;\n  position: fixed;\n  overflow: hidden;\n  border-bottom: 2px solid rgba(249, 249, 249, 1);\n  .android-header {\n    position: relative;\n  }\n  .header-edit {\n    top: 0;\n    right: 20px;\n    color: #fff;\n    font-size: 28px;\n    position: absolute;\n  }\n  .header-search {\n    display: flex;\n    padding: 26px 0;\n    position: relative;\n    background: #fff;\n    justify-content: center;\n  }\n  .icon-sousuo {\n    top: 50%;\n    left: 50px;\n    font-size: 24px;\n    position: absolute;\n    transform: translateY(-50%);\n  }\n  .input {\n    width: 700px;\n    height: 64px;\n    outline: none;\n    font-size: 24px;\n    padding-left: 70px;\n    border-radius: 12px;\n    box-sizing: border-box;\n    background: rgba(249, 249, 249, 1);\n  }\n}\n.card-collection-list {\n  overflow: hidden;\n}\n.card-item-wrapper {\n  width: 100%;\n  height: 190px;\n  display: flex;\n  padding: 29px 30px;\n  box-sizing: border-box;\n}\n.empty-img {\n  width: 350px;\n  height: 335px;\n  display: block;\n  margin: 40px auto;\n}\n.card-item-selection {\n  width: 80px;\n  display: flex;\n  align-items: center;\n}\n.card-item-radio {\n  width: 40px;\n  height: 40px;\n  display: flex;\n  border-radius: 20px;\n  align-items: center;\n  box-sizing: border-box;\n  justify-content: center;\n  border: 1px solid #c3c3c3;\n  &.active {\n    background: #274ff3;\n  }\n  .icon-dagou1 {\n    color: #fff;\n    font-size: 32px;\n  }\n}\n.card-collection-footer {\n  bottom: 0;\n  width: 100%;\n  display: flex;\n  position: fixed;\n  padding: 15px 30px;\n  background: #fff;\n  box-sizing: border-box;\n  justify-content: space-between;\n  .button {\n    width: 324px;\n    height: 80px;\n    color: #fff;\n    outline: none;\n    font-size: 32px;\n    border-radius: 8px;\n    background: #274ff3;\n    &.del {\n      color: #274ff3;\n      background: #fff;\n      border: 2px solid #274ff3;\n    }\n  }\n}\n</style>"],"sourceRoot":"src/module/card/collection"}]}