      .map(item => item.id)\\n        if (!deletes.length) {\\n          dialogService.alert('请选择需删除的文章')\\n          return false\\n        }\\n        let res = await Article.delArticle(deletes)\\n        if (res.status !== 0) throw new Error(res.message)\\n        this.list = this.list.filter(item => !item.isActive)\\n        dialogService.toast('删除成功')\\n        if (\\n          this.list.length < this.searchParams.limit &&\\n          this.list.length < this.count - deletes.length\\n        ) {\\n          this.searchParams.offset = this.list.length\\n          this.getList()\\n        }\\n      } catch (error) {\\n        dialogService.alert(error.message)\\n      }\\n    },\\n    // 长按开始\\n    loopTouchStart() {\\n      clearInterval(this.loopTime) //再次清空定时器，防止重复注册定时器\\n      this.loopTime = setTimeout(() => {\\n        this.isEdit = true\\n      }, 1000)\\n    },\\n    // 长按结束\\n    loopTouchEnd() {\\n      clearInterval(this.loopTime)\\n    }\\n  },\\n  computed: {\\n    ...mapState({\\n      platform: state => state.system.platform\\n    }),\\n    paddingTop: function() {\\n      if (this.isEdit) {\\n        return '0px'\\n      } else {\\n        return this.platform !== 'wechat' && !this.isEdit ? '198px' : '116px'\\n      }\\n    }\\n  },\\n  components: {\\n    Wrapper,\\n    ArticleItem,\\n    AppHeader,\\n    AppLoading,\\n    AppBottomOperation\\n  }\\n}\\n</script>\\n<style lang=\\\"scss\\\" scoped>\\n.collection {\\n  padding-bottom: 110px;\\n}\\n\\n.no-data {\\n  overflow: hidden;\\n\\n  img {\\n    width: 390px;\\n    position: relative;\\n    margin: 60px auto 30px;\\n    display: block;\\n  }\\n\\n  .goCollection {\\n    position: relative;\\n    margin: auto;\\n    color: #fff;\\n    width: 190px;\\n    height: 70px;\\n    line-height: 70px;\\n    font-size: 30px;\\n    background: #274ff3;\\n    border-radius: 35px;\\n    text-align: center;\\n  }\\n}\\n\\n.header {\\n  top: 0;\\n  width: 100%;\\n  position: fixed;\\n  overflow: hidden;\\n  b