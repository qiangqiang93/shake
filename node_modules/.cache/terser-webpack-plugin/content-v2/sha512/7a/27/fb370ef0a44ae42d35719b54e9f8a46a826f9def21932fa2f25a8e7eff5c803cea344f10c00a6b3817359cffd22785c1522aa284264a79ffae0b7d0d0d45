\\n        dialogService.alert(err.message)\\n      }\\n    },\\n    // 跳转到文章详情\\n    goDetail(id) {\\n      this.$router.push(`/article/detail/${id}`)\\n    },\\n    // 跳转到活动\\n    goActivity(isLogin) {\\n      if (isLogin) {\\n        this.$router.push('/activity/invitation')\\n      } else {\\n        let guider = `user_id=${this.guider || ''}`\\n        let redirect = 'redirect=' + encodeURIComponent(this.redirect)\\n        location.href = `${window.api}/front/oauth?${guider}&${redirect}`\\n      }\\n    }\\n  },\\n  computed: {\\n    ...mapState({\\n      guider: state => state.system.guider,\\n      userInfo: state => state.user.userInfo,\\n      platform: state => state.system.platform,\\n      redirect: state => state.system.redirect\\n    })\\n  },\\n  components: {\\n    Wrapper,\\n    AppHeader,\\n    AppLoading,\\n    ArticleItem\\n  }\\n}\\n</script>\\n\\n<style lang=\\\"scss\\\" scoped>\\n.app-404-contain {\\n  width: 100%;\\n  box-sizing: border-box;\\n  .app-404-headerPng {\\n    width: 100%;\\n    display: flex;\\n    margin-top: 77px;\\n    flex-flow: column;\\n    align-items: center;\\n    justify-content: center;\\n    .app-404-headerPng-img {\\n      width: 304px;\\n    }\\n  }\\n}\\n.app-404-body {\\n  margin-top: 76px;\\n  .app-404-body-title {\\n    color: #444;\\n    font-size: 34px;\\n    padding: 0 30px;\\n    margin-top: 40px;\\n    font-weight: 500;\\n    box-sizing: border-box;\\n    font-family: PingFang-SC-Bold;\\n  }\\n  .app-404-list {\\n    width: 100%;\\n    height: 390px;\\n    position: relative;\\n  }\\n}\\n.app-404-footer {\\n  .app-404-footer-title {\\n    color: #444;\\n    font-size: 34px;\\n    padding: 0 30px;\\n    margin-top: 40px;\\n    font-weight: 500;\\n    margin-bottom: 43px;\\n    box-sizing: border-box;\\n    font-family: PingFang-SC-Bold;\\n  }\\n  .app-404-footer-body {\\n    width: 611px;\\n    margin: 0 auto;\\n    padding: 0 40px;\\n    border-radius: 10px;\\n    border: 1px solid #ddd;\\n    box-shadow: 