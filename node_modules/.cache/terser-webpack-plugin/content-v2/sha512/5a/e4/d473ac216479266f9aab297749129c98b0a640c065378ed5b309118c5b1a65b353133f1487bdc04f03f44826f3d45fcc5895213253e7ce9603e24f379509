     slideIndex: 0, //控制transform距离\\n      slide: {\\n        transform: 'translate3d(0px, 0px, 0px)'\\n      },\\n      paginationStyle: {\\n        bottom: '20px'\\n      },\\n      date: new Date().getTime(),\\n      move: false // 是否移动，用于轮播图切换位置时出现的bug，true时再一次初始化\\n    }\\n  },\\n  mounted() {\\n    if (\\n      this.canEdit &&\\n      (this.item.type === 'slider' ||\\n        this.item.type === 'pics' ||\\n        this.item.type === 'imgText')\\n    ) {\\n      let flag = null\\n      flag = this.item.children.some(item => {\\n        // 解决只有item.type = 'text' 时img为null没有item.img.src就会报错\\n        let src = item.img ? item.img.src : item.img\\n        return src\\n      })\\n      this.item.isAddSuperLink = flag\\n    }\\n    //初始化轮播\\n    this.initSlide()\\n  },\\n  methods: {\\n    ...mapActions(['updatePreviewGoods', 'updateTempDatas']),\\n    //slide初始化\\n    initSlide() {\\n      if (this.item.type === 'slider') {\\n        //获取长度\\n        let length = 0\\n        this.item.children.forEach(item => {\\n          item.isSliderItem ? length++ : ''\\n        })\\n        this.slideLength = length\\n        //家超链接操作\\n        let hasLink = this.item.children.some(it => {\\n          return it.img.superLink.text.length > 0\\n        })\\n        this.paginationStyle = hasLink ? { bottom: '60px' } : { bottom: '20px' }\\n        //自动轮播\\n        if (this.canEdit) {\\n          clearInterval(this.autoSlide)\\n        } else {\\n          let that = this\\n          this.autoSlide = setInterval(() => {\\n            that.pageChange(that.autoDriction)\\n            if (this.slideIndex == -this.slideLength + 1) {\\n              that.autoDriction = 1\\n            }\\n            if (this.slideIndex == 0) {\\n              that.autoDriction = -1\\n            }\\n          }, 2000)\\n        }\\n      }\\n    },\\n    // 轮播改变页\\n    pageChange(direction) {\\n      let newIndex = this.slideIndex + direction\\n      if (newIndex === 1 || newIndex === -this.slideLength) {\\n        this.slide = {\\n          transform: `translate3d(${this.slideIndex * 750}px, 0px, 0px)`\\n        }\\n        return\\n      }\\n      this.slideIndex += direction\\n      let distance = newIndex * 750\\n      this.slide = {\\n        transform: `translate3d(${distance}px, 0px, 0px)`\\n      }\\n    },\\n    //轮播图滑动处理\\n    touchstart(event) {\\n      this.posX = event.touches[0].pageX\\n      this.distance = this.slideIndex * 750\\n      this.slide.transition = 'unset'\\n    },\\n    //轮播图滑动处理\\n    touchmove(event) {\\n      let posX = event.changedTouches[0].pageX\\n      let distance = posX - (this.posX ? this.posX : 0)\\n      if (distance > 0) {\\n        let last = this.distance + distance\\n        this.slide.transform = `translate3d(${last}px,0px,0px)`\\n      } else {\\n        let last = this.distance + distance\\n        this.slide.transform = `translate3d(${last}px,0px,0px)`\\n      }\\n    },\\n    //轮播图滑动处理\\n    touchend(event) {\\n      if (this.posX > event.changedTouches[0].pageX) {\\n        this.posX - event.changedTouches[0].pageX > 200\\n          ? this.pageChange(-1)\\n          : ((this.slide.transform = `translate3d(${this.slideIndex *\\n              750}px,0px,0px)`),\\n            (this.slide.transition = 'all 0.3s'))\\n      } else if (this.posX < event.changedTouches[0].pageX) {\\n        event.changedTouches[0].pageX - this.posX > 200\\n          ? this.pageChange(1)\\n          : ((this.slide.transform = `translate3d(${this.slideIndex *\\n              750}px,0px,0px)`),\\n            (this.slide.transition = 'all 0.3s'))\\n      }\\n    },\\n    // tools组件操作\\n    toolsAction(e) {\\n      this.$parent.$parent.childOperation(e[0], e[1])\\n    },\\n    // 组件操作\\n    componentOperate(type, index) {\\n      this.move = true\\n      this.$parent.$parent.childOperation(type, index)\\n    },\\n    // 删除banner\\n    delswiperImg(index) {\\n      this.$parent.$parent.delswiper(this.index, index)\\n    },\\n    // 上传图片\\n  