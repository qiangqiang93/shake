', // 验证码\\n      tips: '获取验证码', // 验证码提示\\n      active: true, // 是否可点击发送验证码\\n      timer: false,\\n      isRequest: false // 是否在请求\\n    }\\n  },\\n  beforeMount() {\\n    // 如果有token，则跳转到首页\\n    let token = this.system.token\\n    if (token) {\\n      this.$router.replace('/index')\\n    }\\n  },\\n  computed: {\\n    ...mapState(['system'])\\n  },\\n  methods: {\\n    ...mapActions(['system/setToken', 'system/updateRedirect','system/updateIsPerfect']),\\n    select(index) {\\n      this.selectIndex = index\\n      clearInterval(this.timer)\\n      this.code = ''\\n      this.phone = ''\\n      this.password = ''\\n      this.tips = '获取验证码'\\n      this.active = true\\n    },\\n    // 获取验证码 type发送验证码 1登录 2注册 3改密 4换绑 5设置支付密码\\n    async getCode() {\\n      if (!this.active) return\\n      let check = validate.phone(this.phone)\\n      if (!check.status) {\\n        dialogService.toast(check.msg)\\n        return false\\n      }\\n      try {\\n        let res = await System.sendValidateCode(this.phone, 1)\\n        if (res.status !== 0) throw new Error(res.message)\\n        dialogService.toast('发送成功')\\n        this.countdown()\\n      } catch (err) {\\n        dialogService.toast(err.message)\\n      }\\n    },\\n    // 倒计时\\n    countdown() {\\n      let second = 60\\n      this.active = false\\n      this.tips = second + 's'\\n      this.timer = setInterval(() => {\\n        second--\\n        this.tips = second + 's'\\n        if (second === 0) {\\n          clearInterval(this.timer)\\n          this.active = true\\n          this.tips = '获取验证码'\\n        }\\n      }, 1000)\\n    },\\n    // 登录\\n    async login() {\\n      if (this.isRequest) return false\\n      try {\\n        let phoneValidate = validate.phone(this.phone)\\n        if (!phoneValidate.status) {\\n          throw new Error(phoneValidate.msg)\\n        }\\n        let res = null\\n        if (this.selectIndex == 1) {\\n          // 密码登录\\n          if (!this.password.length) {\\n            throw new Error('密码不能为空')\\n          }\\n          if (this.password.length < 6 || this.password.length > 12)\\n            throw new Error('密码长度为6-12位')\\n          this.isRequest = true\\n          res = await Login.pwdLogin({\\n            phone: this.phone,\\n            password: this.password\\n          })\\n        } else {\\n          // 短信登录\\n          if (!this.code.length) {\\n            throw new Error('验证码不能为空')\\n          }\\n          this.isRequest = true\\n          res = await Login.codeLogin({\\n            phone: this.phone,\\n            code: this.code\\n          })\\n        }\\n        if (res.status === -1) {\\n          throw new Error(res.message)\\n        }\\n        this['system/setToken'](res.data.token)\\n        await User.getProfile()\\n        this.isRequest = false\\n        // 是否完善信息\\n        if (!res.data.is_perfect) {\\n          this.$router.push('/information')\\n          return false\\n          // debugger\\n        } else {\\n          this['system/updateIsPerfect'](res.data.is_perfect)\\n        }\\n        // 考虑转发的后\\n        let a = this.system.redirect\\n        if (a) {\\n          this['system/updateRedirect'](null)\\n          this.$router.push(a)\\n        } else {\\n          this.$router.push('/index')\\n        }\\n      } catch (error) {\\n        this.isRequest = false\\n\\n        dialogService.toast(error.message)\\n      }\\n    },\\n    goRegiste() {\\n      let guider = this.system.guider || ''\\n      let baseUrl = `${window.api}/front/oauth?user_id=${guider}`\\n      let redirect = `&redirect=${encodeURIComponent(this.system.redirect)}`\\n      if (guider) {\\n        location.href = baseUrl + redirect + '&poster=1'\\n      } else {\\n        location.href = baseUrl + redirect\\n      }\\n    }\\n  },\\n  beforeRouteLeave(to, from, next) {\\n    // this.system.token\\n    let nameArr = ['/forgetPwd', '/register']\\n    if (nameArr.indexOf(to.path) === -1) {\\n      if (!this.system.token) {\\n